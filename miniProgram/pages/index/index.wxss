  border-radius: 0 50rpx 50rpx 0;
}

.status_bar .status {
  flex: 1;
  text-align: center;
  line-height: 70rpx;
  padding: 0 16rpx;
  background-color: #efefef;
  color: #555;
  font-size: 26rpx;
  font-weight: 600;
}

.status_bar .actived {
  background-color: #dc0805;
  color: #fff !important;
}

.status_bar .actived .text {
  color: #fff !important;
  /* font-weight: initial !important; */
}

