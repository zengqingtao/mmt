/* 轮播 */

.indexAd {
  position: relative;
}

.swipers swiper {
  width: 100%;
  height: 400rpx;
  box-sizing: border-box;
}

.swipers.newSwipers swiper {
  height: 296rpx;
  /* box-shadow: 0 0 5px rgba(0, 0, 0, 0.2) */
}

.swipers.changeIndex {
  display: flex;
  justify-content: center;
}

.swipers.changeIndex swiper {
  height: 260rpx;
}

.swipers swiper swiper-item {
  background-size: contain;
  background-repeat: no-repeat;
  background-position: center;
}

.swipers swiper swiper-item image {
  width: 702rpx;
  margin: 0 auto;
}

.swipers .countDownDemo {
  position: absolute;
  bottom: 40rpx;
  right: 24rpx;
  border-radius: 100px 0px 0px 100px;
  padding: 10rpx 20rpx;
  text-align: center;
}

.swipers .countDownDemo .countDown-text {
  font-size: 34rpx;
  color: #fff;
  margin-bottom: 16rpx;
}

.time-text {
  background-color: #fff;
  display: flex;
  min-width: 60rpx;
  padding: 0 6rpx;
  box-sizing: border-box;
  height: 60rpx;
  justify-content: center;
  align-items: center;
  font-size: 44rpx;
  font-weight: 600;
  color: #550b10;
  box-shadow: 0 1px 3px #550b10;
}

.doc-text {
  font-size: 38rpx;
  font-weight: bold;
  text-align: center;
  color: #fff;
  margin: 0 8rpx;
}

.adImg {
  display: block;
  width: 750rpx;
  height: 400rpx;
}

.newAdimg {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 702rpx;
  height: 296rpx;
  box-sizing: border-box;
  border-radius: 4px;
  margin: 0 24rpx;
}

.changeAdimg {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 260rpx;
  box-sizing: border-box;
}

/* service_title */

.services_title {
  text-align: center;
  background-color: #fff;
  padding-top: 25rpx;
}

.services_title .iconfont {
  display: block;
  /* height: 48rpx;
  line-height: 48rpx; */
  font-size: 30rpx;
  margin-right: 8rpx;
  color: #ffe150;
}

.services_title .nums-name {
  height: 56rpx;
  line-height: 56rpx;
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  position: relative;
}

/* bar */

.barList {
  padding: 0 25rpx;
  /* padding-bottom: 25rpx; */
  display: flex;
  flex-wrap: wrap;
  align-content: flex-start;
  box-sizing: border-box;
}

.barList .bar_item {
  position: relative;
  flex: 0 0 25%;
  padding: 25rpx 0;
  font-size: 26rpx;
  color: #555;
  text-align: center;
}

.barList .bar_item.bar_item_5 {
  flex: 0 0 20%;
}

.barList .bar_item button {
  display: block;
  font-size: 26rpx;
  line-height: 1;
  padding: 0;
  background: #fff;
}

.barList .bar_item image {
  width: 100rpx;
  height: 100rpx;
  display: block;
  margin: 0 auto;
  margin-bottom: 12rpx;
}

.barList .bar_item text {
  line-height: 40rpx;
  height: 40rpx;
  display: block;
  color: #555;
}

