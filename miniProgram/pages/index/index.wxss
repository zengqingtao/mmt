/*  index.wxss */
@import "../listdetailTemplate/listdetailTemplate.wxss";

page {
  height: auto !important;
  background: transparent;
  background: #f5f6fa !important;
  overflow-x: hidden;
}

.page {
  overflow-x: hidden;
}

/* 自定义导航栏 */

.navbar-box {
  width: 100%;
}

.navbar-menu {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  padding: 0 24rpx;
  background-color: #ffe150;
  z-index: 99997;
  display: flex;
  align-items: flex-end;
  box-sizing: border-box;
}

.navbar-menu .navbar-left {
  position: relative;
  height: 46px;
  line-height: 46px;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
}

.navbar-menu .navbar-left .pickup {
  background-color: rgba(255, 255, 255, 0.5);
  flex: 1;
  padding: 0 12rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 50rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
  color: #333;
  position: relative;
}

.navbar-menu .navbar-left .pickup .icon-btn_dizhix {
  font-size: 28rpx;
  color: #333;
  flex: 1;
}

.navbar-menu .navbar-left .pickup .pickup_content {
  max-width: 200rpx;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.navbar-menu .navbar-left .pickup .right_icon {
  color: #eb3c39;
  flex: 1;
  width: 60rpx;
  height: 60rpx;
  background-position: center;
  background-size: 14rpx 22rpx;
  background-repeat: no-repeat;
}

.navbar-menu .navbar-conter {
  position: absolute;
  left: 0;
  bottom: 0;
  text-align: center;
  width: 100%;
  height: 46px;
  line-height: 46px;
  z-index: -1;
}

/* 轮播背景 */

.swiper-header-bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 375rpx;
  z-index: 0;
}

/* 搜索框 */

.indexSearch {
  position: relative;
  top: 0;
  z-index: 99;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
}

.indexSearch .pickup {
  flex: 1;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  display: flex;
  align-items: center;
  /* padding: 0 16rpx; */
  font-size: 26rpx;
  color: #333;
  position: relative;
}

.indexSearch .pickup .pickup_content {
  /* display: block; */
  max-width: 450rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.indexSearch .pickup .icon-btn_dizhix {
  font-size: 28rpx;
  color: #333;
  flex: 1;
  /* position: absolute;
  left: -35rpx;
  top: 0; */
}

.indexSearch .pickup .right_icon {
  color: #eb3c39;
  flex: 1;
  height: 60rpx;
  background-position: center;
  background-size: 14rpx 22rpx;
  background-repeat: no-repeat;
}

.indexSearch .searchButton {
  width: 432rpx;
  height: 60rpx;
  line-height: 60rpx;
  opacity: 0.8;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #555;
  background-color: #e6e6e6;
}

