/* pages/newExclusive/newExclusive.wxss */

page {
  height: auto !important;
  background-color: #fff !important;
  padding-bottom: 100rpx !important;
}

view {
  box-sizing: border-box;
}

.toCart {
  position: fixed;
  bottom: 40rpx;
  right: 20rpx;
  z-index: 100;
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 50%;
  font-size: 50rpx;
  font-weight: 600;
  text-align: center;
  border: 1rpx solid #dcdcdc;
  background-color: #fff;
  color: #000;
}

.ad {
  width: 100%;
  height: auto;
  margin-bottom: 16rpx;
}

.ad .ad_img {
  width: 100%;
  /* height: 100%; */
  display: block;
  background: #eee;
}

.coupon_wrap {
  width: 724rpx;
  height: 144rpx;
  margin: 0 auto;
  margin-bottom: 16rpx;
  background: #eb3c39;
  border-radius: 8rpx;
  padding: 12rpx 10rpx;
  box-sizing: border-box;
  position: relative;
}
.coupon_wrap .cover_img{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
  width: 44rpx;
  height: 144rpx;
}
.scroll_wrap {
  width: 500rpx;
  height: 120rpx;
  white-space: nowrap;
}

.scroll_wrap .coupon {
  display: inline-block;
  width: 216rpx;
  height: 120rpx;
  background-size: cover;
  background-repeat: no-repeat;
}

.scroll_wrap .coupon .price {
  color: #bb803b;
  font-size: 52rpx;
  font-weight: 600;
  text-align: center;
}

.scroll_wrap .coupon .price .small {
  font-size: 20rpx;
  color: #bb803b;
}

.scroll_wrap .coupon .tab {
  font-size: 20rpx;
  color: #fff;
  text-align: center;
  margin: 0 auto;
  margin-top: 4rpx;
}

.scroll_wrap .coupon .tab .text {
  min-width: 130rpx;
  height: 30rpx;
  border-radius: 20rpx;
  background: #bb803b;
  padding: 0 10rpx;
}

.coupon_wrap .total_box {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 20rpx;
}

.coupon_wrap .total_box .total_price {
  font-size: 55rpx;
  color: #fff;
  text-align: center;
}

.coupon_wrap .total_box .small {
  font-size: 20rpx;
}

.coupon_wrap .total_box .get_btn {
  width: 160rpx;
  height: 42rpx;
  background-size: cover;
  background-repeat: no-repeat;
}

.ac_new_person {
  width: 720rpx;
  height: 560rpx;
  margin: 0 auto;
  margin-bottom: 16rpx;
  background-size: cover;
  background-repeat: no-repeat;
  box-sizing: border-box;
  padding: 80rpx 6rpx 20rpx 6rpx;
  position: relative;
}
.ac_new_person .icon-btn_xianshifengqiangguizex{
  position: absolute;
  color: #fff;
  top: 35rpx;
  right: 20rpx;
}
.common_good {
  margin: 0 auto;
  margin-bottom: 16rpx;
  padding: 0 15rpx;
  background-color: #fff;
}

.good_list_wrap .good_list {
  width: 100%;
  height: 100%;
  display: flex;
}

.good_list_wrap .good_list .good_info {
  background-color: #fff;
  width: 232rpx;
  padding-bottom: 12rpx;
  margin-bottom: 16rpx;
  border-radius: 8rpx;
  overflow: hidden;
}

.good_list_wrap .good_list .good_pic {
  width: 232rpx;
  height: 232rpx;
  background: #fff;
  position: relative;
}

.inequacy {
  width: 100%;
  height: 100%;
  position: absolute;
  background:rgba(0,0,0,0.3) ;
  top: 0;
  left: 0;
  z-index: 10;
  display: flex;
  align-items: center;
  justify-content: center;
  /*background-color: transparent !important;*/
}
.inequacy image{
  width: 158rpx!important;
  height:66rpx!important;
  background: none!important;
}

.good_list_wrap .good_list .good_pic image {
  display: block;
  width: 100%;
  height: 100%;
}

.good_list_wrap .good_list .good_pic .goods_lable {
  width: 75%;
  height: 75%;
  position: absolute;
  top: 0;
  left: 0;
}

.good_list_wrap .good_list .prom_info {
  height: 34rpx;
  line-height: 34rpx;
  padding: 0 10rpx;
  background-color: #ff0041;
  font-size: 18rpx;
  color: #fff;
  text-align: center;
  position: absolute;
  bottom: -36rpx;
  left: 0;
}

/* 折扣标签 */

.good_list_wrap .good_list  .good_pic .discount_label {
  display: block;
  line-height: 34rpx;
  position: absolute;
  top: 0;
  left: 0;
  color: #fff;
  font-size: 24rpx;
  padding: 0 5rpx;
  margin-left: 5rpx;
  margin-top: 5rpx;
}

.good_list_wrap .good_list .title {
  width: 232rpx;
  box-sizing: border-box;
  height: 72rpx;
  line-height: 36rpx;
  margin: 12rpx 0;
  padding: 0 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 28rpx;
}

.good_list_wrap .good_list .price {
  color: #dc0805;
  font-weight: 600;
  font-size: 32rpx;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
}

.good_list_wrap .good_list .small {
  font-size: 18rpx;
}

.good_list_wrap .good_list .false_price {
  text-decoration: line-through;
  color: #b5b5b5;
  font-size: 26rpx;
  font-weight: 400;
  margin-left: 6rpx;
}

.good_list_wrap .good_list .add_cart {
  width: 212rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  background: #fff;
  margin: 0 auto;
  border-radius: 30rpx;
}

.good_list_wrap .good_list .add_cart button {
  display: block;
  width: 100%;
  height: 100%;
  line-height: 54rpx;
  padding: 0;
  background-color: transparent;
  font-size: 26rpx;
  color: #eb3c39;
}

.good_list_wrap .good_list .addToShopCart {
  background-size: 100% 100%;
  background-repeat: no-repeat;
}

/*SKU弹窗*/

.drawer_attr_box {
  width: 100%;
  box-sizing: border-box;
  position: fixed;
  bottom: 0;
  left: 0;
  opacity: 0;
  z-index: 1200;
  background: #fff;
}

.close_icon {
  position: absolute;
  right: 30rpx;
  top: 30rpx;
  width: 60rpx;
  height: 60rpx;
}

.close_icon wx-image {
  position: absolute;
  right: 0rpx;
  top: 0rpx;
  width: 30rpx;
  height: 30rpx;
}

.drawer_attr_content {
  padding-bottom: 100rpx;
  overflow-x: hidden;
  overflow-y: scroll;
}

.drawer_attr_content .buy_now_btn {
  width: 100%;
  height: 96rpx;
  line-height: 96rpx;
  background-color: #eb3c39;
  border-radius: 0;
  position: absolute;
  bottom: 0;
  right: 0;
  font-size: 34rpx;
  color: #fff;
}

.text_center {
  width: 90%;
  box-sizing: border-box;
  height: 168rpx;
  padding: 40rpx;
  padding-left: 190rpx;
  margin: 0 30rpx;
  border-bottom: 1rpx #e4e4e4 solid;
}

.product-SKU {
  width: 100%;
  vertical-align: top;
  display: inline-block;
  margin-left: 24rpx;
}

.product-SKU .product-price {
  font-size: 30rpx;
  font-weight: 700;
  line-height: 36rpx;
  color: #f23030;
}

.product-SKU .product-price .small {
  color: #b5b5b5;
  font-size: 18rpx;
  margin-left: 10rpx;
  text-decoration: line-through;
}

.product-SKU .product-choose {
  font-size: 28rpx;
  line-height: 1;
  color: #767676;
  padding-top: 18rpx;
}

.drawer_image {
  position: absolute;
  top: -40rpx;
  left: 30rpx;
  width: 180rpx;
  height: 180rpx;
  background-color: rgb(247, 244, 244);
  border: 1rpx solid #e4e4e4;
  overflow: hidden;
}

.drawer_image image {
  width: 180rpx;
  height: 180rpx;
}

.drawer_desc {
  font-size: 28rpx;
  margin: 20rpx;
  height: 150rpx;
  width: 100%;
}

.group-name {
  font-size: 26rpx;
  box-sizing: border-box;
  color: #979797;
  text-align: left;
  line-height: 50rpx;
}

.group-name.attr_num_txt {
  width: 70%;
}

/*分类选择*/

.drawer_attr_box .title {
  padding: 10rpx 0rpx;
  margin: 10rpx 0;
  border-left: 4rpx solid #ccc;
}

/*每组属性的主盒子*/

.attr-margin {
  padding-top: 20rpx;
  max-height: 500rpx;
  overflow: hidden;
}

.attr_main {
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
}

.attr_main.sku-height {
  max-height: 400rpx;
  overflow: auto;
}

/*数量*/

.group-click {
  width: 30%;
  height: 50rpx;
  line-height: 50rpx;
  text-align: right;
  align-self: center;
}

.attr-amount {
  width: 90%;
  padding: 0;
  display: flex;
  padding-top: 40rpx;
  margin: 10rpx 30rpx 40rpx 30rpx;
  border-top: 1rpx #e4e4e4 solid;
}

.amount-button {
  color: #555;
  background-color: #f7f7f7;
  border: none;
}

.amount-input {
  border: none;
}

/*全部属性的主盒子*/

.commodity_attr_list {
  display: inline-block;
  width: 100%;
  background: #fff;
}

.attr_box {
  width: 100%;
}

/*属性名*/

.attr_name {
  float: left;
  display: block;
  font-size: 26rpx;
  line-height: 50rpx;
  border-radius: 8rpx;
  color: #555;
  padding: 0 20rpx;
  margin: 0 20rpx 20rpx 0;
  min-width: 94rpx;
  border-width: 0.5px;
  overflow: hidden;
  text-align: center;
  box-sizing: border-box;
  background-color: #e4e4e4;
  border: 1rpx #e4e4e4 solid;
}

.attr_name.active {
  border: 1rpx #dc0805 solid;
  color: #dc0805;
  background-color: #ffebeb;
}

.attr_name.disable, .attr_name.active.disable {
  border: 1rpx #ddd solid;
  background-color: #fff;
  color: #ddd;
}

/* sku弹窗结束 */
