.right {
  flex: 1;
  height: 100%;
  overflow: hidden;
  background: #fff;
}
.tab_bar{
  display: flex;
  width: 590rpx;
  background-color: #fff;
  height: 60rpx;
  line-height: 60rpx;
  position: relative;
}
.tab_bar .tab_item{
  flex: 1;
  font-size: 28rpx;
  line-height: 60rpx;
  text-align: center;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  color:#555;
  font-weight: 500;
}
.tab_bar .change_bar{
  font-weight: 500;
  color:#303030!important;
}
.tab_bar .icon-paixu{
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  right: 20%;
  top: 17rpx;
  z-index: 999;
}
.tab_bar .change_bar::after{
  content: '';
  display: block;
  width: 50rpx;
  height: 4rpx;
  /*font-weight: 600;*/
  color:#303030!important;
  background-color: #FFE150;
  position: absolute;
  left: 50%;
  bottom:2rpx;
  transform: translateX(-50%);
}
.ac_bb{
  border-top: 1rpx solid #eee;
  width: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
}
.right .content{
  height: 80%;
}
.ac_img{
  padding: 16rpx 24rpx;
  background-color: #fff;
}

.ac_img image{
  width: 550rpx;
  height: 180rpx;
}
.right{
  position: relative;
}

.good_list{
  padding-left: 16rpx;
}
.good_list .good{
  position: relative;
  display: flex;
  padding: 16rpx 0;
  border-bottom: 2rpx solid #f0f0f0;
  background-color: #fff;
}
.good_list .good .good_pic{
  flex: 0 0 144rpx;
  width: 144rpx;
  height: 144rpx;
  position: relative;
  background-size: 100%;
  background-repeat: no-repeat;
}
.good_list .good_pic .goods_lable{
  /* width: 70rpx !important;
  height: 24rpx !important; */
  position: absolute;
  top: 0;
  left: 0;
}
.good_list .good .good_pic .inequacy {
  width: 100%;
  height: 100%;
  position: absolute;
  background:rgba(0,0,0,0.3) ;
  top: 0;
  left: 0;
  z-index: 10;
  display: flex;
  align-items: center;
  justify-content: center;
  /*background-color: transparent !important;*/
}
.inequacy image{
  width: 126rpx!important;
  height:52rpx!important;
  background: none!important;
}
.good_list .good .good_pic image{
  width: 100%;
  height: 100%;
}
.good .good_pic .discount_label{
  padding: 0 5rpx;
   margin-left: 5rpx;
  margin-top: 5rpx;
  color: #fff;
  font-size: 24rpx;
  line-height: 32rpx;
  display: inline-block;
  position: absolute;
  left:0;
  top: 0;
}
.good .tab{

  background-color: #FFE150;
  color: #555555;

  font-size: 20rpx;
  height: 32rpx;
  line-height: 32rpx;
  width: 60rpx;
  text-align: center;
  display: inline-block;
  margin-right:5rpx;
  vertical-align: middle;
}
