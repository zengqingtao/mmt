page{
  overflow: hidden;
}
.cant-scroll {
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: absolute;
  top: 0;
}

.section {
  position: absolute;
  width: 100%;
  height: 100%;
  overflow: hidden;
  top: 80rpx;
  display: flex;
}


/* 搜索 */

.search {
  width: 100%;
  padding: 8rpx 0rpx 16rpx 0;
  background: #fff;
  box-sizing: border-box;
}

.ui-searchbar {
  box-sizing: border-box;
  border-radius: 50rpx;
  background: #efefef;
  height: 60rpx;
  line-height:48rpx;
  color: #555;
  font-size: 28rpx;
  width: 702rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin:0 auto;
}
.icon-img_sousuox{
  line-height: 60rpx;
}
.left {
  flex: 0 0 160rpx;
  height: 100%;
  width: 160rpx;
  background: #f7f7f7;
  text-align: center;
  font-size: 24rpx;
  color: #888;
}
.left view {
  display: block;
  height: 112rpx;
  line-height: 112rpx;
  /*border-bottom: 2rpx solid #f0f0f0;*/
  width: 160rpx;
}
.left .actived{
  background-color: #fff;
  position: relative;
  font-weight:600;
}
