/* pages/flashSale/flashSale.wxss */

page {
  background-color: #fff!important;
}

.ad {
  width: 100%;
  height: 360rpx;
}

.ad image {
  width: 100%;
  height: 100%;
}

.good_wrap .good_pic{
  width: 200rpx;
  height: 200rpx;
  flex: 0 0 200rpx;
  position: relative;
  background-size:100%;
  background-repeat:no-repeat;

}
.good_wrap .good_pic .inequacy {
  width: 200rpx;
  height:200rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0.3);
}
.good_wrap .good_pic .inequacy image{
  width: 158rpx;
  height:66rpx;
}
.status_bar {
  display: flex;
  height: 110rpx;
  width: 100%;

}

.status_bar .status {
  flex: 1;
  text-align: center;
  font-size: 32rpx;
  line-height: 40rpx;
  padding: 16rpx 0;
  color: #555555;
  background-color: #f2f2f2;
}

.status_bar .status .text {
  font-size: 28rpx;
}

.status_bar .actived {
  font-weight: 500;
  background-color: #ffe150;

  color: #303030;
}

/* 倒计时 */

.countDown {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  line-height: 50rpx;
  height: 50rpx;
  color: #555;
  position: relative;
}

.countDown text {
  margin-right: 10rpx;
}

/* 商品列表 */

.good_wrap {
  position: relative;
  display: flex;
  padding: 16rpx 0;
  width: 700rpx;
  margin: 0 auto;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f2f2f2;
}

.good_wrap .good_pic {
  width: 200rpx;
  height: 200rpx;
  flex: 0 0 200rpx;
}

.good_wrap .good_pic image {
  width: 100%;
  height: 100%;
}

.good_wrap .good_info {
  padding: 0 24rpx 0 30rpx;
  flex: 1;
  position: relative;
  box-sizing: border-box;
}
.good_wrap .sale_bar{
  width: 250rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border: 1rpx solid #aaa;
  border-radius: 20rpx;
  display: flex;
  font-size: 20rpx;
  overflow: hidden;
}
.good_wrap .sale_bar_enough{
  width: 250rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border: 1rpx solid #aaa;
  background: #aaa;
  border-radius: 20rpx;
  display: flex;
  font-size: 20rpx;
  overflow: hidden;
}
.good_wrap .sale_bar_enough .bar_enough{
  width: 100%;
  text-align: center;
  color: #fff;
}
.good_wrap .sale_bar .left{
  flex: 1;
  background: #aaa;
  color: #fff;
}
.good_wrap .sale_bar .right{
  flex: 1;
  color: #555;

}
