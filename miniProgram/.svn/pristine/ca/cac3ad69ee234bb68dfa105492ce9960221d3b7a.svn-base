.good_wrap .good_info .good_name {
  width: 480rpx;
  height: 85rpx;
  font-size: 30rpx;
  color: #545454;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin-bottom: 10rpx;
}
.good_wrap .good_info .good_tag {
  font-size: 18rpx;
  padding: 0 10rpx;
  line-height: 1.6;
  color: #555;
  border-width: 2rpx;
  border-style: solid;
  border-color: #ffe150;
  background-color: #ffe150;
  border-radius: 20rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}
.good_wrap .good_info .good_num {
  font-size: 28rpx;
}

.good_wrap .good_num {
  font-size: 28rpx;
  color: #fb4;
}

.good_wrap .middle_pos {
  /* position: absolute;
  top: 75rpx;
  left: 30rpx; */
}

.good_wrap .good_price {
  color: #dc0805;
  font-size: 20rpx;
  margin-bottom: 5rpx;
  /* position: absolute;
  left: 30rpx;
  bottom: 30rpx; */
}

.good_wrap .good_price .larger {
  font-size: 32rpx;
  font-weight: 600;
  margin-right: 10rpx;
}

.good_wrap .good_price .small {
  font-size: 20rpx;
  color: #b5b5b5;
  text-decoration: line-through;
}

.good_wrap .get_btn {
  width: 175rpx;
  height: 64rpx;
  position: absolute;
  top: 118rpx;
  right: 28rpx;
  font-weight: 500;
}

.good_wrap .get_btn .btn1 {
  width: 100%;
  height: 100%;
  color: #555555;
  background-color: #ffe150!important;
  border-radius: 30rpx;
  line-height: 62rpx;
  font-size: 28rpx;
  padding: 0 14rpx;
}

.good_wrap .get_btn .btn2 {
  width: 100%;
  height: 100%;
  color: #555;
  background-color: #fff;
  border: 2rpx solid #555;
  line-height: 62rpx;
  font-size: 28rpx;
  padding: 0 14rpx;
   border-radius: 40rpx;
}

.good_wrap .get_btn .btn3 {
  width: 100%;
  height: 100%;
  color: #555;
  background-color: #fff;
  border: 2rpx solid #555;
  border-radius: 32rpx;
  line-height: 62rpx;
  font-size: 28rpx;
  padding: 0 14rpx;
}

.countDown-line-lf {
  width: 140rpx;
  height: 0.5rpx;
  background-color: #e5e5e5;
  position: absolute;
  left: 0;
}

.countDown-line-rt {
  width: 140rpx;
  height: 0.5rpx;
  background-color: #e5e5e5;
  position: absolute;
  right: 0;
}

.flashSale-from-goods{
  position: absolute;
  top: 0;
  left: 0;
}
.flashSale-from-goods button{
  width: 750rpx;
  height: 230rpx;
  background-color: transparent;
}
