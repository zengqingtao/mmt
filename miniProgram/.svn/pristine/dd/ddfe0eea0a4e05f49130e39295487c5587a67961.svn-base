.row{
  flex-direction: row;
  align-items: center;
}
.flex {
  flex: 1;
}

.cashCoupon{
  width: 100%;
  height: 100%;
  background-size: cover;
  background-repeat: no-repeat;
  position: relative;
  box-sizing: border-box;
}
.content{
  width: 100%;
  height: 927rpx;
  background-size: cover;
  background-repeat: no-repeat;
  position: fixed;
  top: 110rpx;
}
.cashCoupon_box{
  position: relative;
  height:927rpx;
}
.cashCoupon_header{
  width: 100%;
  height: 150rpx;
  text-align: center;
  padding-top: 80rpx;
}
.cashCoupon_header .head_portrait{
  width: 96rpx;
  height: 96rpx;
  border: 1px solid #9f0000;
  border-radius: 50%;
  margin: 0 auto;
  overflow: hidden;
  /* margin-top: 80rpx; */
}
.head_portrait image{
  width: 100%;
  height: 100%;
  background-size: cover;
  background-repeat: no-repeat;
}
.title{
  font-size: 24rpx;
  height: 45rpx;
  line-height: 45rpx;  
  color: #9f0000;
}
.title .title_name{
  color: #ffd288;
}
.title_pic{
  width: 100%;
  height: 100rpx; 
  line-height: 100rpx;
  text-align: center;
  /* padding: 23rpx 0 */
}
.title_pic image{
  width: 386rpx;
  height: 54rpx;
  background-size: cover;
  vertical-align: middle;
}
.cashCoupon_main{
  height: 355rpx;  
}
.card_bg{
  width: 544rpx;
  height: 200rpx;
  background-size: cover;
  background-repeat: no-repeat;
  margin: 0 auto;
  margin-bottom: 65rpx;
  position: relative;
}
.coupon_left{
  width: 158rpx;
  height: 200rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.coupon_left .coupon_left_text{
  display: block;
  width: 40rpx;
  height: auto;
  /* writing-mode: tb-rl; */
  text-align: center;
  margin: 0 auto;
  font-size: 32rpx;
  color: #eb3c39;
}
.coupon_right{
  text-align: center;
  width: 386rpx; 
  /* height: 200rpx; */
}
.coupon_right .coupon_right_price{
  font-size: 96rpx;
  color: #dc0805;
  height:115rpx;
}
.coupon_right_price .symbol{
  font-size: 36rpx;
}
.coupon_right_text{
  font-size: 24rpx;
  color: #aaa
}
.card_bg .coupon_time{
  position: absolute;
  bottom: -42rpx;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
  width: 100%;
}
.cashCoupon_bottom{
  width: 640rpx;
  height: 362rpx;
  background-size: cover;
  background-repeat: no-repeat;
  position: absolute;
  bottom: 0;
  left: 55rpx;
}
.cashCoupon_bottom_btn{
  width: 496rpx;
  height: 112rpx;
  background-size: cover;
  background-repeat: no-repeat;
  margin: 0 auto;
  text-align: center;
  line-height: 112rpx;
  color: #895e1e;
  position: absolute;
  bottom: 65rpx;
  left: 50%;
  transform: translateX(-50%)
}
.cashCoupon_bottom_btn:active{
  opacity: 0.9;
}