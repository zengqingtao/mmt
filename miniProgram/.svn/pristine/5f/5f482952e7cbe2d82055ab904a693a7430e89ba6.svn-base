/*********** Dialog ***********/

.mask {
    position: fixed;
    z-index: 101;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.6);
    width: 100%;
}

.dialog {
    z-index: 1079;
    position: fixed;
    top: 30%;
    left: 50%;
    width: 80%;
    max-width: 554rpx;
    -webkit-transform: translate(-50%, -30%);
    transform: translate(-50%, -30%);
    background-color: rgba(255, 255, 255, 0.9);
    text-align: center;
    border-radius: 10rpx;
    overflow: hidden;
}

.dialog .dialog-content {
    position: relative;
    width: 100%;
    padding: 96rpx 0 74rpx 0;
    color: #030303;
    font-size: 34rpx;
    overflow: hidden;
    line-height: 34rpx;
    box-sizing: border-box;
}

.dialog .dialog-content:after {
    content: " ";
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    height: 1rpx;
    border-top: 1rpx solid #d5d5d6;
    color: #d5d5d6;
}

.dialog .dialog-closs {
    position: absolute;
    top: 18rpx;
    right: 18rpx;
}

.dialog .dialog-closs img {
    width: 36rpx;
}

.dialog .dialog-button {
    position: relative;
    width: 100%;
    overflow: hidden;
    text-decoration: none;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

.dialog .dialog-button .button {
    float: left;
    position: relative;
    display: block;
    width: 50%;
    box-sizing: border-box;
    color: #030303;
    font-size: 32rpx;
    font-weight: normal;
    text-align: center;
    padding: 30rpx 0;
}

.dialog .dialog-button .button.left {
    font-weight: normal;
}

.dialog .dialog-button .button.right {
    font-weight: bold;
    color: #fdaa17;
}

.dialog .dialog-button .button.right:after {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    width: 1rpx;
    bottom: 0;
    border-left: 1rpx solid #d5d5d6;
    color: #d5d5d6;
}

.confirm-form .item-textarea {
    position: relative;
    display: block;
    width: 100%;
    box-sizing: border-box;
}

.confirm-form .item-textarea .inputnum {
    font-size: 26rpx;
    color: #999;
    padding: 28rpx 0;
    padding-right: 32rpx;
    background: #fff;
    position: relative;
    width: 100%;
    text-align: right;
    box-sizing: border-box;
}

.confirm-form .item-textarea input {
    display: block;
    width: 100%;
    height: 120rpx;
    min-height: 120rpx;
    padding: 26rpx 28rpx;
    box-sizing: border-box;
    background-color: #fff;
    border: none;
    text-align: left;
    overflow: hidden;
    color: #767676;
    font-size: 28rpx;
    font-weight: normal;
    line-height: 32rpx;
    word-break: break-all;
}

