/* pages/user/returnSQA.wxss */
.sqa-list-box {
  position: relative;
  margin-top: 88rpx;
}

.sqa-list-box .box_type {
  background: #fff;
  padding: 52.8rpx 26.4rpx;
  margin-bottom: 30rpx;
}

.sqa-list-box .box_title {
  display: block;
  font-size: 35rpx;
  line-height: 32.64rpx;
  padding-bottom: 51.8rpx;
  font-weight: 600;
  color: #060606;
}

.sqa-list-box .menu-p-title {
  position: relative;
  display: block;
  color: #010101;
  font-size: 32rpx;
  padding-bottom: 11rpx;
  line-height: 45rpx;
  padding-left: 44rpx;
}

.sqa-list-box .menu-p-title text {
  position: absolute;
  top: 0;
  left: 0;
  font-size: 32rpx;
}

.sqa-list-box .menu-p-text {
  display: block;
  font-size: 32rpx;
  line-height: 45rpx;
  padding-left: 44rpx;
  padding-bottom: 55rpx;
  color: #6B6B6B;
  word-wrap: break-word;
}
.box_type .refundProcessImg{
  height: 60rpx;
  width: 100%;
  margin-bottom: 30.8rpx;
}
.box_type .refundsImg{
  height: 150rpx;
  width: 100%;
  margin-bottom: 30.8rpx;
}
.box_type .exchangeImg{
  height: 240rpx;
  width: 100%;
  margin-bottom: 30.8rpx;
}
