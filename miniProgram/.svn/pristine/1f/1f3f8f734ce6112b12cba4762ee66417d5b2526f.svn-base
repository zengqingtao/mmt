.node-number{
  text-align: center;
  background-color: #ffffff;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  flex: 1;
  padding: 0 20rpx;
}
.node-number .text{
  font-size: 28rpx;
  border: 1rpx #555555 solid;
  padding: 0 20rpx;
}
.node-number .number{
  font-size: 40rpx;
  color: #555555;
  margin-left: 40rpx;
  margin-right: 40rpx;
  line-height: 46rpx;
}
.node-number .number .i{
  font-size: 26rpx;
}
.node-number .picker-select{
  padding: 0 10rpx;
  font-size: 26rpx;
  min-width: 80rpx;
  border: 1rpx solid #aaa;
}
.searchbox {
  background-color: #ffffff;
  padding: 30rpx 25rpx;
}
.dataInpt {
  width: 230rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #555555;
  margin-top: 20rpx;
  font-size: 30rpx;
  border: 1rpx #555555 solid;
  padding: 0 15rpx;
  border-radius: 8rpx;
}
.dataInpt.disabled{
  color: #f2f2f2;
  border-color: #f2f2f2
}
.searchInput {
  width: 450rpx;
  height: 100rpx;
  margin-top: 20rpx;
  border-radius: 8rpx;
  margin-left: 20rpx;
  font-size: 30rpx;
  border: 1rpx #555555 solid;
  padding: 0 80rpx 0 15rpx;
  position: relative;
}
.clear-input{
  position: absolute;
  right: 24rpx;
  top: 50%;
  color:#dbd9d9;
  display: flex;
  align-items: center;
  transform: translateY(-50%)
}
.list-pad {
  padding: 0 25rpx;
  padding-bottom: 40rpx;
  background-color: #ffffff;
}
.list-box {
  margin-bottom: 20rpx;
  background-color: #eeeeee;
  border-radius: 8rpx;
  padding: 45rpx 25rpx;
  overflow: hidden;
  position: relative;
}
.list-item {
  padding: 10rpx 0;
  color: #555555;
  font-size: 30rpx;
  display: flex;
  align-items: center;
}
.list-time {
  position: absolute;
  right: 0;
  top: 0;
  background-color: #1DA1F2;
  color: #ffffff;
  padding: 10rpx;
  font-size: 24rpx;
}
.list-btn-box{
  position: absolute;
  right: 24rpx;
  bottom:32rpx;
}
.list-btn-box .list-btn{
  background-color: #1DA1F2;
  color: #ffffff;
  padding: 0 10rpx;
  font-size: 28rpx;
  border-radius: 0;
  margin-bottom: 10rpx;
}
.list-status {
  position: absolute;
  left: 0;
  top: 0;
  background-color: #EB3C39;
  color: #ffffff;
  padding: 10rpx;
  font-size: 24rpx;
}

.number-model-box{
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  -webkit-transform: translate(-50%,-50%);
  width: 650rpx;
  background-color: #ffffff;
  z-index: 1001
}
.number-model-box .header-text{
  text-align: center;
  font-size: 40rpx;
  height: 100rpx;
  line-height: 100rpx;
}
.number-model-box .header-text i{
  color: #EB3C39;
  margin: 0 10rpx;
  font-size: 48rpx;
}
.number-model-box .header-sale-text {
  font-size: 30rpx;
  color: #555555;
  text-align: center;
  justify-content: center;
  padding-bottom: 25rpx;
  display: flex;
  position: relative;
  align-items: center;
  border-bottom: 1rpx solid #f2f2f2;
}
.number-model-box .header-sale-text i{
  color: #EB3C39;
  font-size: 48rpx;
}
.number-model-box .model-main{
  margin: 20rpx 0;
  width: 100%;
  text-align: center;
  font-size: 30rpx;
}
.number-model-box .model-main .main-title{
  flex: 1;
}
.number-model-box .model-main .main-number{
  flex: 1;
}
.number-model-box .model-scroll{
  height: 740rpx;
}
.number-model-box .model-list{
  width: 100%;
  font-size: 32rpx;
  padding: 10rpx 32rpx 18rpx 24rpx;
  min-height: 88rpx;
  justify-content: center;
  position: relative;
  align-items: center;
}
.number-model-box .model-list:nth-child(2n){
  background-color: #f2f2f2
}
.number-model-box .model-list .list-name{
  width: 60%;
}
.number-model-box .model-list .list-name.red{
  color: red;
}
.number-model-box .model-list .list-sale{
  width: 22%;
}
.number-model-box .model-list .list-sale .i{
  font-size: 26rpx;
  background-color: #EB3C39;
  color: #ffffff;
  padding: 10rpx 0;
  text-align: center;
}
.number-model-box .model-list .list-number{
  width: 18%;
  text-align: center;
  color: #EB3C39;
  font-size: 38rpx;
}
.color-blue{
  color: #1DA1F2;
  font-size: 34rpx;
}
.call-icon{
  margin-left: 30rpx;
  width: 140rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color:#1DA1F2;
  color:#fff;
  height:60rpx;
  font-size: 24rpx;
  border-radius: 34rpx;
}
.right-arrow{
  position: absolute;
  right: 10rpx;
  top: 46%;
  transform: translateY(-50%);
}
.showMore{
  color:#EB3C39;
  position: absolute;
  right: 24rpx;
  top:40%;
  transform: translateY(-50%);
}
.placeholder-font{
  font-size: 26rpx
}
.screen-box{
  width: 100%;
  padding: 20rpx 24rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.screen-box .screen-item{
  width: 100%;
  border: 2rpx solid #e6e6e6;
  padding:10rpx 24rpx;
  box-sizing: border-box;
  position: relative;
  display: flex;
  justify-content: space-between;
}

.screen-box .screen-item .screen-msg{
  width: 87%;
  align-items: center;
}
.screen-box .screen-item .screen-msg .screen-title{
  font-size:24rpx;
  color:#aaa;
  font-weight: 500;
}
.search-disabled{
  border-color: #f2f2f2;
}
.placeholder-disabled{
  color:#f2f2f2;
}
.screen-box .screen-item .screen-msg .screen-news{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-left: 20rpx;
  width: 80%;
}
.screen-box .screen-item .screen-msg .screen-news .goods-name{
  width: 70%;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  text-align: left;
  overflow: hidden;
}
.screen-box .screen-item .screen-msg .screen-news .goods-num{
  width: 30%;
  text-align: right;
  font-size: 24rpx;
}
.screen-box .screen-item .screen-msg .screen-news .goods-num i{
  color: #EB3C39;
  font-size: 38rpx;
}
.screen-box .screen-item .screen-close{
  position: absolute;
  right: 24rpx;
  top:50%;
  transform: translateY(-50%);

  display: flex;
  align-items: center;
  color:#303030;
}
.screen-box .screen-item .screen-close .close-icon{
  font-size: 46rpx;
}
