/* components/loding.wxss */
.loading{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999999;
  width: 100%;
  height: 100vh;
}
.loading .loading-bg{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 140rpx;
  height: 100rpx;
  background-color: rgba(0, 0, 0, 0.3);
  border-radius:6px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.loading .loading-bg .image-loading{
  width: 80rpx;
  height: 80rpx;
}