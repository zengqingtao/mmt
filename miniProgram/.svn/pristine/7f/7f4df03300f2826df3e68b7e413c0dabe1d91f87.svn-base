.order-info .info-text {
    display: flex;
    padding-bottom: 14rpx;
    color: #979797;
    font-size: 24rpx;
    line-height: 40rpx;
    align-items:center;
}
.info-text .add-time{
  color:#333;
}
.order-info .info-Clip{
    /* position: absolute;
    bottom: 42rpx;
    right: 26rpx; */
    display: inline-block;
    margin-left: 20rpx;
}
.order-info .info-Clip button{
    height: 40rpx;
    line-height: 38rpx;
    font-size: 24rpx;
    padding: 0 10rpx;
}

.order-info .info-price text {
    font-size: 32rpx;
    line-height: 32rpx;
}
/* 地址 */
.order-address {
    position: relative;
    display: block;
    width: 100%;
    padding: 24rpx 80rpx;
    margin-bottom: 16rpx;
    box-sizing: border-box;
    background-color: #fff;
}

.order-address .address-tab{
    color: #ffe150;
    position: absolute;
    left: 20rpx;
    top: 50%;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    display: block;
    box-sizing: border-box;
}

.order-address.toumargin {
    margin-top: 26rpx;
}

.order-address .address-contact {
    position: relative;
    width: 100%;
    box-sizing: border-box;
    color: #979797;
    font-weight: normal;
    text-align: left;
}

.order-address .info-leftshift {
    position: relative;
    left: -3rpx;
}

.order-address .address-contact span {
    color: #333;
    font-size: 28rpx;
    font-weight: normal;
    text-align: left;
    line-height: 28rpx;
}

.order-address .address-detail {
    width: 100%;
    box-sizing: border-box;
    color: #979797;
    font-weight: normal;
    text-align: left;
    line-height: 34rpx;
}

.order-address .address-detail span {
    color: #333;
    font-size: 28rpx;
    font-weight: normal;
    text-align: left;
    line-height: 34rpx;
}
.order-address .storeManager{
    position: absolute;
    color: #4C93FD;
    font-size: 26rpx;
    right: 24rpx;
    bottom: 30rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}
.order-address .storeManager .tel_icon{
    display: block;
    width: 30rpx;
    height: 30rpx;
    margin-right: 5px;
}

/*  */
.order-summury {
    position: relative;
    display: block;
    width: 100%;
    padding: 38rpx 24rpx;
    margin: 16rpx 0;

    box-sizing: border-box;
    background-color: #fff;
    overflow: hidden;
}

.order-summury .summury-total {
    display: block;
    overflow: hidden;
    color: #030303;
    padding-top: 34rpx;
    font-size: 28rpx;
    line-height: 1;
    font-weight: normal;
    border-top: 2rpx solid #f0f0f0;
}

.order-summury .summury-total .total-price {
    float: right;
    display: block;
    color: #eb3c39;
    font-size: 28rpx;
    /* padding-right: 26rpx; */
}

.order-summury .summury-detail {
    margin-bottom: 24rpx;
    display: block;
    color: #555;
    font-size: 24rpx;
    font-weight: normal;
    line-height: 28rpx;
}

.order-summury .summury-detail .total-price {
    float: right;
    display: block;
    color: #333;
    font-size: 28rpx;
    /* padding-right: 26rpx; */
    font-weight: 500;
}
.comment{
  width: 100%;
  padding: 24rpx;
  background-color: #fff;
  margin-top: 16rpx;
  display: flex;
  align-items: center;
  color:#555;
  font-size: 24rpx;
}
.icon_rmb{
  font-size: 24rpx;
}
.footer-fix{
    width: 100%;
    background: #fff;
    text-align: right;
    padding: 20rpx 0;
    height: 60rpx;
}
.footer-fix .footer-button{
    display: inline-block;
    min-width: 180rpx;
    height: 60rpx;
    margin-right: 20rpx;
    font-size: 24rpx;
    font-weight: 600;
    text-align: center;
    align-items: center;
    line-height: 60rpx;
    box-sizing: border-box;
    border-radius: 30rpx;

}
button[type=default]{
    border: 1rpx solid #aaa;
    background-color: #fff;
    color: #555;
}
.gohomeImage{
    width: 14rpx;
    height: 24rpx;
    padding-left: 10rpx;
}



.qrCode{
    text-align: center;
    background: #fff;
    padding: 32rpx 0;
    margin-bottom: 20rpx;
}
.qrCode .qr{
    width: 328rpx;
    height: 328rpx;
    margin: 0 auto;
}
.qrCode .qr image{
    width: 100%;
    height: 100%;
}
.qrCode .code{
    font-size: 40rpx;
    line-height: 70rpx;
}
.qrCode .code .small{
    font-size: 30rpx;
}
.qrCode .des{
    font-size: 25rpx;
    line-height: 30rpx;
    border: 1rpx dashed #979797;
    width: 520rpx;
    margin: 0 auto;
    padding: 20rpx 0;
}



/* 弹出窗 */
.action-modal {
    width: 100%;
    height: auto;
    overflow: hidden;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 2000;
    background: #fff;
    padding-bottom: 30rpx;
}

/* 下拉原因 */
.action-modal .title{
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 36rpx;
}
.action-modal .modal-inner {
    height: 300rpx;
}
.action-modal .pick_item{
    font-size: 32rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
}
.action-modal .select_item{
    font-size: 36rpx;
    height: 100rpx;
    line-height: 100rpx;
}

