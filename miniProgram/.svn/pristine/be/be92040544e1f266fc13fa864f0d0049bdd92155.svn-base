/* fix foot button */

.footfixed {
    position: fixed;
    bottom: -100rpx;
    left: 0;
    background: #fff;
    width: 100%;
    height: 96rpx;
    display: flex;
    z-index: 1999;
    /* border-top: 1rpx solid #ddd; */
    opacity: 0;
    transition: 0.15s ease-in all;
}

.footfixed.footShow {
    opacity: 1;
    bottom: 0;
}

.footfixed.footnone {
    opacity: 0;
    bottom: -100rpx;
}

.footfixed .left {
    display: flex;
    width: 216rpx;
    padding: 0 19rpx;
    box-sizing: border-box;
}

.footfixed .submit-icon {
    width: 92rpx;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    position: relative;
}

.footfixed .submit-icon .icon-ico {
    width: 54rpx;
    height: 54rpx;
}

.footfixed .submit-icon .icon-contact {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: transparent;
}

.footfixed .submit-icon .text {
    font-size: 20rpx;
    color: #000;
}

.footfixed .submit-icon .cart-badge {
    position: absolute;
    top: 5rpx;
    right: 5rpx;
    background-color: #ff0041;
    color: #fff;
    width: 35rpx;
    height: 35rpx;
    line-height: 35rpx;
    text-align: center;
    font-size: 20rpx;
    border-radius: 50%;
}

.footfixed .submit-icon .max-length {
    width: 50rpx;
    border-radius: 20rpx;
}

.footfixed .bottom-buttons {
    width: 534rpx;
}

.footfixed .bottom-buttons.team-buttons {
    width: 534rpx;
}

.footfixed .bottom-buttons .submit-button {
    width: 218rpx;
    height: 96rpx;
    line-height: 96rpx;
    border-radius: 0;
    font-size: 32rpx;
    padding: 0 10rpx;
}

.footfixed .bottom-buttons.team-buttons .submit-button {
    width: 200rpx;
}

.footfixed .bottom-buttons .team-button {
    line-height: 1.5;
}

.footfixed .bottom-buttons.team-buttons .team-button-big {
    width: 334rpx;
    line-height: 1.5;
}

.footfixed .bottom-buttons .team-button .team-button-price {
    font-weight: 500;
    font-size: 36rpx;
}

.footfixed .bottom-buttons .team-button .team-button-price .i {
    font-size: 24rpx;
    margin-right: 5rpx;
}

.footfixed .bottom-buttons .team-button .team-button-text {
    font-size: 24rpx;
}

.footfixed .bottom-buttons .submit-button.add-cart {
    background-color: #303030;
    color: #fff;
}

.footfixed .bottom-buttons .submit-button.now-purchase {
    background-color: #ffe150;
    color: #303030;
}

.end-remind {
    background-color: #aaa;
    color: #fff;
}

.footfixed .submit-button-big {
    width: 536rpx;
    height: 96rpx;
    line-height: 96rpx;
    background-color: #ffe150;
    font-size: 36rpx;
    border-radius: 0;
    color: #303030;
}

.footfixed .submit-button-big-team {
    width: 534rpx !important;
}

/* 租机底部按钮样式开始 */

.footfixed .submit-button[disabled] {
    background-color: #eb3c39;
    color: #fff;
    opacity: 0.3;
}

.footfixed .rent-button {
    display: flex;
    width: 436rpx;
    height: 96rpx;
    line-height: 96rpx;
    padding: 0;
    margin: 0;
    border-radius: 0;
    position: absolute;
    top: 0;
    right: 0;
}

.footfixed .rent-button .rent-btn {
    flex: 1;
    color: #555;
    font-size: 24rpx;
}

.yellow_bg {
    background-color: #ffe150;
}

.red_bg {
    background-color: #eb3c39;
}

/* 租机底部按钮样式结束 */

