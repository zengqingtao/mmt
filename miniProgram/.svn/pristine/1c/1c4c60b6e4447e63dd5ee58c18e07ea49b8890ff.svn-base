.shop {
  background: #fff;
  padding: 4%;
  margin-bottom: 10px;
}

.shop checkbox {
  margin-top: 27px;
}

.sh_slt {
  width: 80px;
  height: 80px;
  overflow: hidden;
  border-radius: 5px;
  margin-right: 4%;
  margin-left: 3%;
  float: left;
}

.sp_text {
  float: left;
  line-height: 20px;
  width: 100%;
}

.sp_tit {
  width: 100%;
  overflow: hidden;
  font-size: 14px;
}

.sp_neb {
  width: 100%;
  overflow: hidden;
  font-size: 12px;
  color: #999;
}

.sp_jg {
  width: 100%;
  overflow: hidden;
  font-size: 14px;
  color: #ff8247;
}

.dle {
  color: #999;
  font-size: 12px;
  float: right;
  margin-left: 10px;
}

.dle image {
  width: 18px;
  height: 18px;
  vertical-align: sub;
}

.jk_bottom {
  position: fixed;
  bottom: 0;
  background: #fff;
  width: 100%;
  display: inline-flex;
  border-bottom: 1px solid #eee;
  font-size: 14px;
  color: #999;
}

.jk_bottom checkbox {
  margin: 4% 0 4% 4%;
}

.jk_bottom .heji {
  margin-top: 5.5%;
  width: 30%;
  text-align: right;
}

.jk_bottom .all {
  margin-top: 5.5%;
  padding-left: 2%;
}

.jk_bottom .js_button {
  background: #ff8247;
  float: right;
  color: #fff;
  font-size: 16px;
  text-align: center;
  width: 40%;
  position: absolute;
  right: 0;
  line-height: 50px;
}

.bottom_clear {
  clear: clear;
  height: 1px;
  margin-top: 60px;
}

/*外部容器*/

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

/*顶部信息提示栏*/

.cart-postage {
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 3;
  background: #f6f6f6;
  padding: 27rpx 0 18rpx 21.6rpx;
  box-sizing: border-box;
}

.cart-postage image, .cart-postage .postage-item .item-checkok {
  float: left;
  width: 21.6rpx;
  height: 21.6rpx;
  margin-top: 3.6rpx;
  vertical-align: middle;
}

.cart-postage text, .cart-postage .postage-item .item-txt {
  float: left;
  font-size: 21.6rpx;
  /* line-height: 11px; */
  height: 28.8rpx;
  line-height: 28.8rpx;
  vertical-align: middle;
  color: #767676;
  padding-left: 7.9rpx;
}

.cart-postage .postage-item .item-arrow-right {
}

.cart-postage .postage-item .item-right-btn {
}

/*toptip信息*/

.cart-toptip {
  height: 91rpx;
  line-height: 91rpx;
  background: #fff;
  position: relative;
}

.cart-toptip .toptip-txt {
  font-size: 27.7rpx;
  line-height: 91rpx;
  padding-left: 23.7rpx;
  color: #767676;
}

.cart-toptip .toptip-btn {
  position: absolute;
  top: 0;
  right: 23.7rpx;
  background: none;
  border: 0;
  color: #FF7F95;
  font-size: 27.7rpx;
  line-height: 91rpx;
}

/*商品列表*/

.cart-list {
  /*padding-top: 24rpx;*/
  position: relative;
  display: block;
  width: 100%;
  box-sizing: border-box;
  border-top: #e9e9e9 1px solid;
  border-width: 0.5px;
  padding-bottom: 223rpx;
  padding-bottom: calc(223rpx + env(safe-area-inset-bottom));
}
.cart-bg-yellow{
  /* background-color: #e9e9e9; */
}
.cart-bg-white{
  background-color: #fff;
}
.cart-list .cart-tip .go-buy{
  color: #fd804d;
  font-size: 26rpx;
  position: absolute;
  right: 24rpx;
  top: 50%;
  transform: translateY(-50%);
}
.cart-list .cart-tip .go-buy .icon-btn_xiangyoudajiantoux{
  font-size: 24rpx;
  margin-left: 4rpx;
}
.cart-list .cart-tip{
  padding: 24rpx 24rpx 32rpx 82rpx;
  line-height: 30rpx;
  position: relative;
}
.item-box .good_name{
  font-size:28rpx;
  line-height:1.5;
  color:#545454;
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
}
.item-box .prom_tab{
  font-size:18rpx;
  padding:0 10rpx;
  line-height:1.6;
  color:#dc0805;
  border-width:2rpx;
  border-style:solid;
  border-color:#dc0805;
  border-radius:20rpx;
  margin-right:10rpx;
  vertical-align:middle;
}
.cart-list .list-item {
  position: relative;
  width: 100%;
  height: 244rpx;
  box-sizing: border-box;
  padding: 23rpx 0;
  /*margin-top: 16rpx;*/
}

.cart-list .list-item .item-prev {
  width: 97rpx;
}

.cart-list .list-item .item-box {
  position: relative;
  width: 100%;
  box-sizing: border-box;
  border-width: 0.5px;
  /* padding-bottom: 23.7rpx; */
}

.cart-list .list-item .item-box.noborder {
  border-bottom: 0;
}

.cart-list .list-item .item-after {
  margin-left: 23.7rpx;
  width: 339.3rpx;
  height: 54.9rpx;
}

.cart-list .list-item .item-after .sku-btn {
  position: relative;
  top: 0;
  display: block;
  width: 100%;
  height: 100%;
  background: #fff;
  border: 1px #d0d0d0 solid;
  border-radius: 3.6rpx;
  border-width: 0.5px;
  padding-left: 19.8rpx;
  padding-right: 59.4rpx;
  box-sizing: border-box;
  overflow: hidden;
}

.cart-list .list-item .item-after .sku-btn .info-attr {
  font-size: 23.7rpx;
  color: #767676;
  line-height: 52.2rpx;
  display: -webkit-box;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}

.cart-list .list-item .item-after .sku-btn image {
  position: absolute;
  top: 15.8rpx;
  right: 19.8rpx;
  width: 19.8rpx;
  height: 19.8rpx;
}

.cart-list .list-item .item-checkbox {
  position: absolute;
  top: 50%;
  left: 23rpx;
  transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
  width: 34rpx;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 32rpx;
  /* background-image: url("http://img.mall.surex.cn/cilicili_icons/icon_check_off.png");
  background-repeat: no-repeat;
  background-size: 100% 100%; */
}
.cart-list .list-item .item-checkbox image{
  width: 100%;
  height: 100%;
}
.cart-list .list-item .icon-btn_weixuanzhongx{
  color: #979797;
}
/* .cart-list .list-item .icon-gouxuan{
  color: #eb3d39;
  font-size: 35rpx;
} */
.cart-list .list-item .item-content {
  position: relative;
  padding-left: 79.2rpx;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}

