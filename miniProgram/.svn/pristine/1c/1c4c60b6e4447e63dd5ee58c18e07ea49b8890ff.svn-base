.cart-list .list-item .item-false{
  position: absolute;
  right: 10rpx;
  top: 0;
  width: 124rpx;
  height: 86rpx;
  background-size: contain;
  background-repeat: no-repeat;
}
.cart-list .list-item .item-disable.is-tip {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 23rpx;
  border-radius: 50%;
  width: 32rpx;
  min-width: 32rpx;
  height: 32rpx;
  border: 1px #979797 solid;
  background: #f4f4f4;
}

.cart-list .list-item .item-photo {
  position: relative;
  width: 160rpx;
  height: 160rpx;
  box-sizing: border-box;
  overflow: hidden;
  background-color: #fff;
}

.cart-list .list-item .item-photo span {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-position: 50%;
  background-repeat: no-repeat;
  background-size: cover;
  -webkit-background-size: cover;
  background-color: rgb(247, 244, 244)
}

.cart-list .list-item .item-info {
  position: relative;
  display: block;
  margin-left: 23.7rpx;
  max-width: 345rpx;
}

.cart-list .list-item .item-info .info-name {
  display: -webkit-box;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: #333333;
  width: 90%;
  font-size: 28rpx;
  line-height: 1.4;
  font-weight: normal;
  text-align: left;
  box-sizing: border-box;
}

.cart-list .info-buywarning {
  position: absolute;
  left: 201.9rpx;
  bottom: 91.8rpx;
  font-size: 23.7rpx;
  color: #ff4222;
  width: 336rpx;
  text-align: center;
  line-height: 1;
}

.cart-list .info-buywarning.add {
  top: 74.1rpx;
}

.cart-list .list-item .item-info .info-name.color {
  color: #aaaaaa;
}

.cart-list .list-item .item-info .info-attr {
  width: 90%;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-line-clamp: 1;
  box-sizing: border-box;
  color: #888;
  font-size: 24rpx;
  line-height: 30.7rpx;
  margin-top: 8rpx;
  font-weight: normal;

}

.cart-list .list-item .item-info .info-attr.attr-edit {
  width: 118%;
  /*text-overflow: ellipsis;
	overflow: hidden;*/
  display: -webkit-box;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-line-clamp: 1;
  box-sizing: border-box;
  text-align: right;
  position: absolute;
  top: 7.9rpx;
  margin-top: 0;
  right: -57.6rpx;
}

.cart-list .list-item .item-info .info-attr.color {
  color: #aaaaaa;
}

.cart-list .list-item .item-info .info-price {
  position: absolute;
  top: 140rpx;
  display: block;
  color: #030303;
  font-size: 33.6rpx;
  line-height: 33.6rpx;
  font-weight: normal;
  text-align: left;
  box-sizing: border-box;
}

.cart-list .list-item .item-box .item-del {
  position: absolute;
  top: 0rpx;
  right: 24rpx;
  color: #bbb;
  font-size: 30rpx;
}
.cart-list .list-item .item-box .item-del button{
  background-color: transparent;
  padding: 0;
  line-height: 1;
}

.cart-list .list-item .item-info .info-price .price-normal {
  font-weight: 500;
  color: #dc0805;
}

.cart-list .list-item .item-info .info-price .price-normal .price-coin,
.cart-list .list-item .item-info .info-price .price-normal .price-val {
  font-size: 40rpx;
  padding-right: 3.9rpx;
}

.cart-list .list-item .item-info .info-price .del {
  font-size: 25.7rpx;
  line-height: 25.7rpx;
  text-decoration: line-through;
  -moz-text-decoration-color: #979797;
  color: #979797;
}

.cart-list .list-item .item-info .info-price.color {
  color: #aaaaaa;
}

.cart-list .list-item .info-amount {
  position: absolute;
  right: 23.7rpx;
  bottom: 23.7rpx;
  display: block;
  border-radius: 24rpx;
  overflow: hidden;
  border: 2rpx solid #f7f7f7;
}

.cart-list .list-item .info-amount.amount-edit {
  position: absolute;
  /* left: 201.9rpx; */
  right: 23.7rpx;
  bottom: 23.7rpx;
  display: block;
}


/*底部栏*/

.cart-submit {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 100;
  width: 100%;
  height: 100rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-top: 2rpx #f0f0f0 solid;
}

.cart-submit .submit-select {
  height: 100%;
  width: 25%;
  box-sizing: border-box;
  color: #979797;
  font-size: 22rpx;
  text-align: left;
  line-height: 100rpx;
  padding-left: 80rpx;
  background-color: #fff;
  /* border-top: 1px #e4e4e4 solid; */
}

.cart-submit .submit-select .select-checkbox {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 23rpx;
  width: 34rpx;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 32rpx;
  /* background-image: url("http://img.mall.surex.cn/cilicili_icons/icon_check_off.png"); */
  /* background-repeat: no-repeat;
  background-size: 100% 100%; */
}
.cart-submit .submit-select .select-checkbox image{
  width: 100%;
  height: 100%;
}
.cart-submit .submit-select .icon-btn_weixuanzhongx{
  color: #979797;
}
.cart-submit .submit-select .icon-gouxuan{
  color: #FFE150;
  font-size: 34rpx;
}
.cart-submit .submit-total {
  height: 100%;
  width: 45%;
  box-sizing: border-box;
  font-weight: normal;
  text-align: right;
  padding-right: 17.8rpx;
  background-color: #fff;

}

.cart-submit .submit-total .total-price {
  /*line-height: 100rpx;*/
  /*vertical-align: center;*/
  box-sizing: border-box;
  display: flex;
  /*align-items: center;*/
  flex-direction:column;

  justify-content: center;
  height:100rpx;
  /*padding-top: 20rpx;*/

}

.cart-submit .submit-total .total-price .i {
  color: #030303;
  font-size: 24rpx;
  font-weight: normal;
  display: inline-block;
  vertical-align: top;
  /*height:80rpx;*/
  /*line-height:68rpx;*/
}

.cart-submit .submit-total .total-price .price {
  color:#e71f19;
  font-size:32rpx;
  font-weight:bold;
  position:relative;
  display: inline-block;
  vertical-align: top;
}

.cart-submit .submit-total .total-price .price-yf {
  color: #979797;
  font-size: 22rpx;
  vertical-align:top;
  padding-left: 10rpx;
}

.cart-submit .submit-total.active {
  opacity: 1.0;
}

.cart-submit .submit-total .bottom-info {
  position: absolute;
  bottom: 8.2;
  font-size: 21.7rpx;
  white-space: nowrap;
  right: 56.3rpx;
  line-height: 21.7rpx;
  color: #999;
}

.cart-submit .submit-total .total-checkbox {
  color: #404245;
  font-size: 32.4rpx;
  font-weight: normal;
  text-align: left;
}

.cart-submit .submit-button {
  float: left;
  position: relative;
  width: 30%;
  height: 100%;
  box-sizing: border-box;
  font-weight: normal;
  background-color: #f7f7f7;
  line-height: 90rpx;
  text-align: center;
}

.cart-submit .submit-button span {
  color: #f6f6f6;
  font-size: 31.6rpx;
  line-height: 31.6rpx;
  letter-spacing: 1.8rpx;
  vertical-align: middle;
}

.cart-submit .submit-button.active {
  opacity: 1.0;
}

.cart-submit .submit-button.dark {
  color: #fff;
  background-color: #9b9b9b;
}

.cart-submit .submit-button.dark.active {
  color: #fff;
  background-color: #f9ae08;
}

.cart-submit .submit-button.del {
  color: #fff;
  background-color: #bbb;
}

.cart-submit .submit-button.del.active {
  color: #fff;
  background-color: #f9ae08;
}

.cart-submit .submit-button.win {
  color: #fff;
  background-color: #bbb;
}

.cart-submit .submit-button.win.active {
  color: #fff;
  background-color: #333;
}

