/* 最近浏览 */

.tx-productLis-margin {
  margin-top: 21.7rpx;
  background-color: #fff;
}

.tx-productLis-header view {
  font-size: 32.4rpx;
  position: relative;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-align: center;
  padding-left: 21.6rpx;
  padding-right: 21.6rpx;
  -webkit-box-orient: vertical;
  -webkit-box-pack: center;
  text-align: center;
  letter-spacing: 7.2rpx;
  height: 99rpx;
  line-height: 99rpx;
  font-weight: 400;
}

.cart-del-button {
  font-size: 14px;
  height: 30px;
  line-height: 30px;
  width: 336rpx;
  margin: 0 auto;
  border-radius: 50rpx;
  margin-bottom: 28rpx;
  margin-top: 28rpx;
  text-align: center;
  color: #aaaaaa;
  position: relative;
  font-weight: 500
}

.cart-del-button:after {
  display: block;
  content: " ";
  width: 200%;
  height: 200%;
  position: absolute;
  top: 0;
  left: 0;
  border: 1px solid #aaa;
  -webkit-transform: scale(0.5);
  -webkit-transform-origin: 0 0;
  /*border-radius: 6px;*/
  border-radius: 60rpx;
  padding: 1px;
  -webkit-box-sizing: border-box;
  pointer-events: none;
  z-index: 0;
}


