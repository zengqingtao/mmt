/**app.wxss**/
@import "wxParse/suui/suui.wxss";
@import "iconfont.wxss";

.user-item-checkbox {
  width: 49.68rpx;
  height: 49.68rpx;
  background-image: url('http://img.mall.surex.cn/cilicili_icons/icon_check_off.png');
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.user-active {
  width: 49.68rpx;
  height: 49.68rpx;
  background-image: url('http://img.mall.surex.cn/cilicili_icons/icon_check_on.png');
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

/* 订单商品详情 */

.shop {
  background: #fff;
  /* padding: 4%; */
  margin-top: 22rpx;
  flex-direction: column;
}

.shop checkbox {
  margin-top: 27px;
}

.shop .item_header {
  position: relative;
  box-sizing: border-box;
  width: 100%;
  padding-left: 26.4rpx;
  height: 88rpx;
  background-color: #fff;
  border-bottom: #e9e9e9 2rpx solid;
  border-width: 1rpx;
}

.header_sn {
  float: left;
  position: relative;
  width: 70%;
  display: block;
  color: #767676;
  font-size: 30rpx;
  font-weight: 400;
  text-align: left;
  line-height: 88rpx;
  box-sizing: border-box;
}

.header_status {
  float: left;
  width: 30%;
  display: block;
  color: red;
  padding-right: 26.4rpx;
  font-size: 30rpx;
  font-weight: 400;
  text-align: right;
  line-height: 88rpx;
  box-sizing: border-box;
}

.item_goods {
  position: relative;
  display: block;
  width: 97%;
  height: auto;
  min-height: 250.8rpx;
  margin-left: 26.4rpx;
  box-sizing: border-box;
  border-bottom: #e9e9e9 2rpx solid;
  border-width: 1rpx;
  background-color: #fff;
}

.sh_slt {
  position: absolute;
  left: 0;
  top: 26.4rpx;
  display: block;
  height: 198rpx;
  width: 198rpx;
  overflow: hidden;
}

.df_1 {
  position: relative;
  display: block;
  height: 100%;
  box-sizing: border-box;
  overflow: hidden;
  min-height: 224.4rpx;
  margin-left: 224.4rpx;
  padding-bottom: 30rpx;
}

.sp_text {
  float: left;
  line-height: 20px;
  /* width: 100%; */
  text-align: left;
}

.sp_font {
  position: relative;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  width: 80%;
  font-size: 26rpx;
  font-weight: 400;
  text-align: left;
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
}

.sp_tit {
  -webkit-line-clamp: 2;
  margin-top: 22rpx;
  padding-right: 0 !important;
  color: #030303;
  line-height: 1.4;
}

.sp_neb {
  color: #767676;
  -webkit-line-clamp: 1;
  margin-top: 12.4rpx !important;
  line-height: 30rpx;
}

.sp_jg {
  width: 94%;
  display: block;
  box-sizing: border-box;
  color: #030303;
  font-weight: 700;
  text-align: left;
  font-size: 30rpx;
  line-height: 30rpx;
  margin-top: 16rpx !important;
}

.sp_num {
  position: absolute;
  right: 26.4rpx;
  top: 22rpx !important;
  margin-top: 0 !important;
  font-size: 28rpx;
  color: #767676;
  display: block;
}

.item_footer {
  position: relative;
  display: block;
  height: 95.6rpx;
  padding: 0 27.4rpx 0 0;
  margin-left: 26.4rpx;
  line-height: 96rpx;
  background-color: #fff;
  border-width: 1rpx;
}

.footer_sum {
  color: #e71f19;
  font-weight: 400;
  text-align: right;
  clear: both;
}

.sum_nums {
  color: #767676;
  padding-right: 6rpx;
  font-size: 30rpx;
}

.sum_price {
  font-size: 30rpx;
  color: #767676;
}

.sum_total {
  color: #030303;
  font-weight: 700;
  line-height: 30rpx;
}

.footer_button {
  float: right;
  min-width: 183.6rpx;
  height: 66rpx;
  margin-left: 22rpx;
  background-color: #fff;
  border: 2rpx solid #767676;
  border-width: 1rpx;
  color: #767676;
  border-radius: 6rpx;
  font-size: 28.6rpx;
  font-weight: 400;
  text-align: center;
  line-height: 66rpx;
  box-sizing: border-box;
}

.footer_button.dark {
  border: 2rpx solid #f9ae08;
  border-width: 1rpx;
  color: #f9ae08;
}

/* 分栏 */

.order-filter {
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  height: 88rpx;
  line-height: 88rpx;
  color: #404245;
  font-weight: 400;
  text-align: center;
  box-sizing: border-box;
  background-color: #fff;
  border-bottom: 2rpx #f0f0f0 solid;
  border-width: 1rpx;
  display: -webkit-flex;
  display: flex;
  z-index: 999;
}

.order-filter.fixe-top-bar {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999;
}

.filter-item {
  position: relative;
  display: block;
  flex: 1;
  height: 88rpx;
  font-weight: 500;
  line-height: 88rpx;
  overflow: hidden;
  box-sizing: border-box;
  border-radius: 0;
}

.filter-item text {
  color: #030303;
  font-size: 30.8rpx;
  padding: 0 10.1rpx 17rpx 10.1rpx;
}

.filter-item.active text {
  line-height: 88rpx !important;
  border-bottom: #FFE150 6rpx solid !important;
  color: #FFE150 !important;
}

/* 订单商品列表 */

.order-main {
  position: relative;
  width: 100%;
  box-sizing: border-box;
  background: #fff;
  padding-top: 60rpx;
  padding-bottom: 25rpx;
  /* border-bottom: 2rpx solid #f0f0f0; */
  padding-left: 24rpx;
  padding-right: 24rpx;
}
.date_wrap{
  position: absolute;
  width: 100%;
  top: 0;
  left: 0;
  font-size: 24rpx;
  display: flex;
  justify-content: space-between;
  align-items:center;
  padding: 0 24rpx;
  box-sizing: border-box;
  height: 60rpx;
}
.date_wrap .tab{
  text-align: center;
  width: 110rpx;
  flex: 0 0 110rpx;
  background-color: #FFE150;
  border-radius: 5rpx;
  color: #555;
}
.date_wrap .des{
  color: #FFE150;
}
.order-main .goods-fms {
  position: relative;
  height: 88rpx;
  line-height: 88rpx;
  text-align: left;
  padding-left: 26rpx;
  border-bottom: 1rpx solid #e9e9e9;
}

.order-main .goods-fms .fms-no {
  color: #030303;
  font-size: 26rpx;
  line-height: 1;
}

.order-main .goods-fms .fms-num {
  color: #767676;
  font-size: 26rpx;
  line-height: 1;
}

.order-main .goods-fms .fms-check {
  color: #767676;
  font-size: 26rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
  right: 50rpx;
}

.order-main .goods-fms .fms-arrow {
  position: absolute;
  right: 26rpx;
  top: 50%;
  transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
  width: 14rpx;
}

.order-main .goods-item {
  position: relative;
  height: auto;
  min-height: 160rpx;
  display: block;
  box-sizing: border-box;
  padding-bottom: 15rpx;
}

.order-main .goods-item .item-photo {
  position: absolute;
  top: 0;
  left: 0;
  display: block;
  height: 160rpx;
  width: 160rpx;
  overflow: hidden;
}

.order-main .goods-item .item-photo image {
  width: 100%;
  height: 100%;
  background-color: rgb(247, 244, 244)
}

.order-main .goods-item .item-info {
  position: relative;
  display: block;
  height: 100%;
  min-height: 160rpx;
  box-sizing: border-box;
  margin-left: 200rpx;
  overflow: hidden;
}

.order-main .goods-item .item-info .info-name {
  position: relative;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  width: 70%;
  padding-right: 0 !important;
  box-sizing: border-box;
  color: #030303;
  font-size: 26rpx;
  font-weight: normal;
  text-align: left;
  line-height: 1.7;
}

.order-main .goods-item .item-info .info-attr {
  position: relative;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  width: 80%;
  box-sizing: border-box;
  color: #979797;
  font-size: 24rpx;
  font-weight: normal;
  text-align: left;
  line-height: 1.8;
}

.order-main .goods-item .item-info .info-price {
  position: absolute;
  left: 0rpx;
  bottom: 0;
  box-sizing: border-box;
  color: #030303;
  font-size: 26rpx;
  line-height: 30rpx;
}

.order-main .goods-item .item-info .info-price span {
  margin-left: 1px;
}

.order-main .goods-item .item-info .info-price del {
  font-size: 28rpx;
  line-height: 28rpx;
  color: #bbb;
}

.order-main .goods-item .item-info .confirm-goods-time {
  position: absolute;
  bottom: 0;
}

.order-main .goods-item .item-info .confirm-goods-price {
  font-weight: normal;
}

.order-main .goods-item .item-info .info-return {
  background: #fff;
  position: absolute;
  width: 180rpx;
  height: 60rpx;
  right: 0rpx;
  bottom: 10rpx !important;
  box-sizing: border-box;
  color: #888;
  font-size: 28rpx;
  font-weight: normal;
  text-align: center;
  border: 1px solid #dcdcdc;
  border-width: 0.5px;
  line-height: 60rpx;
  border-radius: 30rpx;
}

.order-main .goods-item .item-info .info-amount {
  position: absolute;
  right: 0;
  top: 10% !important;
  margin-top: 0 !important;
  color: #030303;
  font-size: 27rpx;
  display: block;
}

.order-main .goods-item .item-info .info-amount.amount-up {
  position: absolute;
  right: 26rpx;
  top: 50% !important;
  margin-top: -26rpx !important;
  width: 20%;
  display: block;
  color: #767676 !important;
  font-weight: normal;
}

.order-list .list-item .item-footer {
  position: relative;
  display: block;
  height: 90rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
  padding-right: 26rpx;
  line-height: 90rpx;
  background-color: #fff;
}

.order-list .list-item .item-footer.border-bottom {
  border-width: 0.5px;
}

.order-list .list-item .item-footer.no-border {
  border: 0;
}

.order-list .list-item .item-footer .footer-sum {
  color: #e71f19;
  font-weight: normal;
  text-align: right;
  clear: both;
}

.order-list .list-item .item-footer .footer-sum .sum-text {
  font-size: 26rpx;
  color: #767676;
}

.order-list .list-item .item-footer .footer-sum .sum {
  color: #e71f19;
  font-size: 32rpx;
  font-weight: 700;
}

.order-list .list-item .item-footer .footer-sum .sum-nums {
  color: #767676;
  padding-right: 3px;
  font-size: 26rpx;
}

.order-list .list-item .item-footer .footer-sum .sumlot {
  color: #e71f19;
  font-size: 30rpx;
  line-height: 30rpx;
}

.order-list .list-item .item-footer .footer-button {
  float: right;
  min-width: 180rpx;
  height: 60rpx;
  margin-left: 20rpx;
  border-radius: 20px;
  font-size: 24rpx;
  font-weight: 600;
  text-align: center;
  line-height: 59rpx;
  box-sizing: border-box;
  color:#555;
}

.order-list .list-item .item-footer .footer-button.dark {
  border: 1rpx #f9ae08 solid;
  color: #f9ae08;
}

