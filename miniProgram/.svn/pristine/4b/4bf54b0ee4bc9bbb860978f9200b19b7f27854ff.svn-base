/* 在线客服按钮背景透明 */
.cs_button{
  background: transparent;
}
/* 上拉加载全部加载完成提示 */
.refreshEnd{
  display: block;
  width: 100%;
  height: 140rpx;
  box-sizing: border-box;
  color: #999;
  font-size: 26rpx;
  font-weight: 400;
  line-height: 140rpx;
  /* background: #fff; */
  text-align: center;
  position: relative;
  top: -2rpx;
}

/* back button */

.back-button {
  position: fixed;
  z-index: 1;
  right: 0;
  bottom: 0;
}

 .back-button .backtop, .back-button .gohome {
  display: block;
  width: 94rpx;
  height: 94rpx;
  position: absolute;
  right: 24rpx;
  z-index: 1;
}

.back-button .backtop {
  bottom: 250rpx;
  background-size: cover;
  background-repeat: no-repeat;
  position: absolute;
  display: block;
  width: 88rpx;
  height: 88rpx;
}

.back-button .gohome {
  bottom: 150rpx;
  background-size: cover;
  background-repeat: no-repeat;
  position: absolute;
  display: block;
  width: 75rpx;
  height: 75rpx;
}

.back-button .backtop::after {
  position: absolute;
  display: block;
  content: "";
  width: 110rpx;
  height: 110rpx;
  background-size: cover;
  -webkit-background-size: cover;
  background-repeat: no-repeat;
}



/* 数量输入框 */

.amount-input {
  display: block;
  float: left;
  width: 70rpx;
  height: 50rpx;
  line-height: 50rpx;
  min-height: 50rpx;
  color: #767676;
  font-size: 26rpx;
  font-weight: 400;
  text-align: center;
  background-color: #fff;
  box-sizing: border-box;
}

.amount-button {
  display: block;
  background-color: #f7f7f7;
  float: left;
  box-sizing: border-box;
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  overflow: hidden;
  padding: 0;
}
.amount-button.left{
  border-top-left-radius: 4rpx;
