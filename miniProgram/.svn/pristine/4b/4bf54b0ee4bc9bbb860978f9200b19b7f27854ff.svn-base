.ui-nowrap {
  max-width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

/* 商品列表-一行两列 */

.font-face-ios_Arial {
  font-family: "Avenir Next";
}

.font-face-ios_Avenir {
  font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif;
}

.productList::after {
  content: "";
  display: block;
  clear: both;
}

.goods-item-two {
  position: relative;
  float: left;
  width: 50%;
  max-width: 50%;
  background: #FFF;
  padding-bottom: 29px;
}

.goods-item-two .goods-list-img {
  padding-top: 100%;
  background: #f8f8f8;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  border-right: 2px solid #fff;
  position: relative;
  z-index: 1;
}

.goods-item-two:last-child .goods-list-img{
  border: 0;
}

.goods-list-img span {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  -webkit-background-size: cover;
}

.goods-item-two .goods-list-text {
  color: #030303;
  font-size: 28rpx;
  word-break: break-all;
  padding: 20rpx 9% 0 28rpx;
  line-height: 38rpx;
  margin-bottom: 10rpx;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
}

.goods-item-two .goods-list-price {
  padding-left: 28rpx;
  color: #767676;
  font-size: 26rpx;
  line-height: 1.3;
  font-weight: 300;
  white-space: nowrap;
}

.goods-item-two .goods-list-price.active-color {
  color: #030303;
}

.goods-item-two .goods-list-price .novip-p {
  font-size: 26rpx;
  letter-spacing: 1rpx;
}

.goods-item-two .goods-list-price .novip-y {
  font-size: 26rpx;
  font-weight: 400;
  margin-left: 8rpx;
}

.goods-item-two .goods-list-price .novip-m {
  /* margin-left: -6rpx;  */
  font-size: 28rpx;
  font-weight: 300;
}

.goods-item-two .menu-xl {
  position: absolute;
  bottom: 62rpx;
  right: 24rpx;
  font-size: 24rpx;
  line-height: 1.2;
  color: #767676;
  font-weight: 400;
}

.tx-shopping {
  position: absolute;
  top: 0;
  right: 23.7rpx;
  width: 59.5rpx;
  height: 51.6rpx;
  display: block;
  background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAADwAAAA0CAYAAADWr1sfAAAACXBIW…xgyZg1wA6Ho0wUxS1EVKtUKGr1OhsjZOjy0k8KPXGJx78DAGCtsIYMBCRoAAAAAElFTkSuQmCC) no-repeat;
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
}

.goods-item-two .goods-list-sprice {
  padding-left: 28rpx;
  color: #030303;
  font-size: 26rpx;
  line-height: 1.3;
  font-weight: 300;
  white-space: nowrap;
}

.goods-item-two .goods-list-sprice.active-color {
  color: #767676;
}

.goods-item-two .goods-list-sprice .svip-p {
  font-size: 26rpx;
}

.goods-item-two .goods-list-sprice .svip-y {
  font-weight: 400;
  color: #f23030;
  font-size: 26rpx;
  margin-left: 8rpx;
}

.goods-item-two .goods-list-sprice .svip-m {
  /* margin-left: -6rpx; */
  font-weight: 300;
  color: #f23030;
}

/* 幕布 */

.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0.6);
  /* opacity: 0.2; */
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

/* 选择图片上传 */

.upload-img-box {
  position: relative;
  /* float: left; */
  width: 100%;
  padding: 26rpx 24rpx;
  box-sizing: border-box;
}

.upload-img-box.review {
  padding: 0;
}

.upload-img-box .box-title {
  font-size: 32rpx;
  line-height: 32rpx;
  color: #030303;
  padding-bottom: 22rpx;
}

.upload-img-box .box-title>span {
  color: #bbb;
  font-size: 28rpx;
  line-height: 32rpx;
}

.upload-img-box .box-img {
  position: relative;
  float: left;
  width: 160rpx;
  height: 160rpx;
  margin-right: 22rpx;
  /* margin-bottom: 22rpx; */
}

.upload-img-box .box-img>image {
  float: left;
  width: 100%;
  height: 100%;
}

.upload-img-box .box-img>span {
  position: absolute;
  top: 0;
  left: 0;
  background-size: cover;
  -webkit-background-size: cover;
  background-position: 50%;
  background-repeat: no-repeat;
  float: left;
  width: 100%;
  height: 100%;
}

.upload-img-box .box-img .img-del {
  flex: none;
  position: absolute;
  top: -20rpx;
  right: 0rpx;
  font-size: 40rpx;
  color: #888;
  opacity: 0.8;
}

.upload-img-box>label {
  float: left;
  width: 128rpx;
  height:128rpx;
  /* border: 1rpx #979797 dotted; */
  background-image: url('https://hys-shop.oss-cn-hangzhou.aliyuncs.com/wechat_icons/btn_uploadImg.png');
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: 50% 50%;
}

.upload-img-box>label .uploadImgTxt {
  display: none;
}

.upload-img-box>label.withTxt {
  position: relative;
  background-size: 30% 30%;
  background-position: 50% 35%;
  margin-bottom: 22rpx;
}

.upload-img-box>label.withTxt .uploadImgTxt {
  position: absolute;
  display: block;
  top: 100rpx;
  width: 100%;
  text-align: center;
  font-size: 26rpx;
  color: #bbb;
}

.upload-img-box>label>.images-input {
  display: none;
  font-size: 28rpx;
}

/* end.商品列表-一行两列 */

/* 订单详情客服 */

.order-kf {
  margin-top: 20rpx;
  padding-bottom: 32rpx;
  /* background: #fff; */
  position: relative;
}
.order-kf .phone-kf{
  width: 160rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  background-color: #b5b5b5;
  border-radius: 20rpx;
  margin: 0 auto;
}
.order-kf .phone-kf-time{
  text-align: center;
  margin-top: 24rpx;
}
.order-kf .kf-time {
  font-size: 34rpx;
  line-height: 34rpx;
  color: #030303;
  background: #fff;
  padding: 32rpx 26rpx;
}

.order-kf .kf-type {
  background: #fff;
  padding: 14rpx 26rpx;
  box-sizing: border-box;
}

.order-kf .kf-type .kf-ol {
  border: 1px #767676 solid;
  border-width: 0.8px;
  border-radius: 2px;
  font-size: 32rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #767676;
  margin-right: 20rpx;
}

.order-kf .kf-type .kf-ol.no-margin {
  margin: 0;
}

.order-kf .kf-type .kf-ol image {
  width: 40rpx;
  height: 40rpx;
  padding-right: 6rpx;
}

.order-kf .kf-type .kf-ol text {
  vertical-align: middle;
}

/* 页面数据为空 */

.empty {
  position: relative;
  display: block;
  width: 100%;
  height: 500rpx;
  overflow: hidden;
  background-color: #fff;
}

.empty.address-empty {
  height: 600rpx;
  background: #fff;
}

.empty .empty-item {
  position: absolute;
  left: 0;
  /*top: 15%;*/
  top:365rpx;
  display: block;
  width: 100%;
  height: 200rpx;
  /*margin-top: -180rpx;*/
}

.empty .empty-item .item-logo {
  display: block;
  margin: 0 auto;
  width: 280rpx;
  height: 200rpx;
}

.empty.cart-empty {
  background: #fff;
  height: auto;
}

.empty.cart-empty .empty-item.cart-item-e {
  position: relative;
  top: 0;
  margin-top: 82px;
  padding-bottom: 35px;
  height: auto;
}

.empty.cart-empty .empty-item .item-logo {
  width: 86px;
