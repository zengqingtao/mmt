  background-color: rgba(0,0,0,0.05)
}

/* 售罄弹窗提示 */
.isEnough-curtain {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 99998;
}
.isEnough-popup-model{
  position: fixed;
  top: 40%;
  left: 50%;
  transform: translate(-50%, -40%);
  -webkit-transform: translate(-50%, -40%);
  width: 454rpx;
  background-color: #fff;
  z-index: 99999;
  text-align: center;
  border-radius: 8rpx;
  padding: 46rpx 48rpx;
}
.isEnough-popup-model .title{
  font-size: 28rpx;
  color: #303030;
}
.isEnough-popup-model .msg{
  font-size: 48rpx;
  line-height: 66rpx;
  color: #303030;
  padding-top: 46rpx;
}
.isEnough-popup-model .msg i{
  color: #EB3C39;
  font-size: 52rpx;
}
.isEnough-popup-model button{
  width: 248rpx;
  height: 76rpx;
  line-height: 76rpx;
  background-color: #FFE150;
  color: #905521;
  font-size: 28rpx;
  border-radius: 19px;
  margin-top: 46rpx;
  padding: 0;
  text-align: center;
}

.isEnough-popup-model .closs{
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  -webkit-transform: translateX(-50%);
  bottom: -120rpx;
  width: 72rpx;
  height: 72rpx;
}

