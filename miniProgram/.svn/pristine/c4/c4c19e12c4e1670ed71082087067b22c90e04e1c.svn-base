/* 头部 */

.profile-header {
  width: 100%;
  position: relative;
  background-size: 100% 100%;
  padding: 68rpx 46rpx 0 46rpx;
  box-sizing: border-box;
}

.user_money {
  position: absolute;
  right: 0rpx;
  top: 40%;
  background-color: #fff;
  font-size: 24rpx;
  padding: 0rpx 10rpx;
  border-radius: 50rpx 0 0 50rpx;
  height: 55rpx;
  line-height: 55rpx;
  text-align: center;
  transform: translateY(-50%);
}

.coupon-icon {
  font-size: 32rpx !important;
}

.user_money text {
  font-size: 30rpx;
  color: #dc0805;
  display: inline-block;
  height: 55rpx;
  line-height: 55rpx;
}

.user_info {
  box-sizing: border-box;
  position: relative;
}

/* 充值中心开始 */

/* 充值中心入口按钮 */

.profile-header .user_info .recharge_btn {
  width: 126rpx;
  height: 60rpx;
  background-size: cover;
  position: absolute;
  top: 50%;
  right: 0;
  transform: translateY(-50%);
  padding-left: 70rpx;
  box-sizing: border-box;
  font-size: 22rpx;
  color: #fff;
}

/* 充值中心广告图 */

.recharge_ad {
  width: 700rpx;
  height: 148rpx;
  background-color: #fff;
  margin: 0 auto;
  border-radius: 100rpx;
  padding-top: 24rpx;
  padding-left: 184rpx;
  box-sizing: border-box;
  position: relative;
}

.recharge_ad .icon-gold {
  width: 120rpx;
  height: 120rpx;
  position: absolute;
  left: 40rpx;
  top: 50%;
  transform: translateY(-50%);
}

.recharge_ad .icon-btn_gerenzhongxinyoujiantoux {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 40rpx;
  font-size: 35rpx;
}

.recharge_ad .text1 {
  color: #dc0805;
  font-size: 36rpx;
  font-weight: 600;
  margin-bottom: 24rpx;
}

.recharge_ad .text2 {
  color: #888;
  font-size: 24rpx;
}

/* 充值中心结束 */

.user_pic {
  width: 110rpx;
  height: 110rpx;
  border: 5rpx solid #fff;
  border-radius: 50%;
  background-size: cover;
  background-repeat: no-repeat;
  overflow: hidden;
}

.user_pic image {
  width: 100%;
  height: 100%;
}

.header-right {
  padding-left: 24rpx;
}

.header-name {
  font-size: 28rpx;
  color: #303030;
  font-weight: 600;
  height: auto;
  line-height: 1.5;
  background-color: transparent;
  padding: 0;
}

