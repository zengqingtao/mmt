}
.width90{
  width: 90%;
}
.width10{
  width: 10%;
}
.sendCode {
  background: #030303;
  color: #fff;
}

.ticket-left {
  padding-left: 54rpx;
}
.ticket-left .ticket-title {
  font-size:48rpx;
  line-height:48rpx;
  font-weight:bold;
  padding-bottom:18rpx;
}
.ticket-left .ticket-dec {
  color:#3E3E3E;
  font-size:24rpx;
  line-height:24rpx;
  height:24rpx;
  margin-bottom:10rpx;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  text-overflow:ellipsis;
  overflow:hidden;
  -webkit-line-clamp:1;
}
.ticket-left .ticket-time {
  font-size:24rpx;
  line-height:24rpx;
}
.ticket-button {
  position: relative;
  text-align: center;
  line-height: 130rpx;
  font-size:36rpx;
  font-weight:bold;
}
.button-image {
  position:absolute;
  right:-20rpx;
  bottom:-32rpx;
}
.button-image image {
  width: 148rpx;
  height: 148rpx;
}

.orderListFooter {
  box-sizing: border-box;
  padding: 27.8rpx 32.7rpx;
  display: flex;
  position: relative;
}

.i {
  font-size: 25.8rpx;
  color: #fff;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  float: left;
}
.couponArrow{
  position: absolute;
  right: 0;
  top: 27.8rpx;
  display: flex;
  
}
.unfold .i {
  -webkit-line-clamp: 10;
}

.couponArrow .pa {
  width: 30.8rpx;
  height: 17.8rpx;
  transform: translateY(50%);
  -webkit-transform: translateY(50%);
  right: 0;
}
.emptyCoupon>view{
  text-align: center;
  font-size: 25rpx;
  margin-top: 40rpx;
}
.complete{
  text-align: center;
  font-size: 20rpx;
  margin-top: 49.2rpx;
  margin-bottom: 28rpx;
  color: #7D7D7F;
}
