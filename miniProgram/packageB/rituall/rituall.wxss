.cashgift-exchange {
  box-sizing: border-box;
  background: #f2f2f2;
  padding: 15.8rpx;
}

.exchange-ltinput {
  box-sizing: border-box;
  border: 0;
  font-size: 27.7rpx;
  line-height: 27.7rpx;
  color: #333;
  border-radius: 1.8rpx;
  padding: 15.8rpx 23.76rpx;
  background-color: #fff;
  height: 59.6rpx;
  min-height: initial;
}

.exchange-rtbutton {
  display: inline-block;
  box-sizing: border-box;
  padding: 13.8rpx 31.6rpx;
  border-width: 0.9rpx;
  font-size: 27.7rpx;
  border-radius: 4.8rpx;
  height: 59.6rpx;
