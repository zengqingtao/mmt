  transform: translateX(-50%);
  -webkit-transform: translateX(-50%);
  background-size: 100% 100%;
  background-color: transparent;
}

.header-img-box .header-img-btn-model {
  position: absolute;
  width: 240rpx;
  height: 240rpx;
  left: 50%;
  bottom: 60rpx;
  transform: translateX(-50%);
  -webkit-transform: translateX(-50%);
  background-color: transparent;
}

.header-img-box .img-number {
  position: absolute;
  top: 30%;
  left: 50%;
  transform: translate(-50%, -30%);
  -webkit-transform: translate(-50%, -30%);
  font-size: 160rpx;
  font-weight: 600;
  color: #ffcba8;
  text-shadow: 2px 2px 2px rgba(224, 39, 16, 1);
}
