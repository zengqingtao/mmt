.invitation-record .record-card {
  padding: 20rpx 10rpx;
}

.invitation-record .record-card .card-box {
  padding: 16rpx 14rpx;
  justify-content: center;
  align-items: center;
  background-color: #ffeaea;
  border-radius: 10rpx;
  margin-bottom: 16rpx;
}

.invitation-record .record-card .card-box.urge {
  background-color: #fff;
  border-bottom: 1rpx #f0f0f0 solid;
}

.invitation-record .record-card .card-box .user-logo {
  width: 80rpx;
  height: 80rpx;
  border-radius: 8rpx;
}

.invitation-record .record-card .card-box .msg {
  font-size: 28rpx;
  width: 420rpx;
  margin-left: 24rpx;
  color: #303030;
  display: flex;
}

.invitation-record .record-card .card-box .msg .span {
  width: 100rpx;
  height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.invitation-record .record-card .card-box .msg i {
  font-size: 26rpx;
  margin-left: 24rpx;
}

.invitation-record .record-card .card-box .log {
  font-size: 28rpx;
  width: 420rpx;
  color: #303030;
}

.invitation-record .record-card .card-box .log i {
  font-size: 24rpx;
  margin-left: 24rpx;
}

.invitation-record .record-card .card-box .log i.open {
  color: #eb3c39;
}

.invitation-record .record-card .card-box .log i.urge {
  color: #f6a800;
}

.invitation-record .record-card .card-box .btn {
  width: 128rpx;
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  border-radius: 13px;
  font-size: 26rpx;
  margin-left: 24rpx;
  padding: 0;
}

.invitation-record .record-card .card-box .text-btn {
  font-size: 32rpx;
  color: #eb3c39;
  text-align: right;
  width: 120rpx;
}

.invitation-record .record-card .card-box .text-btn i {
  font-size: 20rpx;
  color: #303030;
}

.invitation-record .record-card .card-box .btn.urge {
  background: rgba(255, 237, 197, 1);
  color: #ff9452;
}

.invitation-record .record-card .card-box .btn.open {
  background: linear-gradient(180deg, rgba(255, 106, 88, 1) 0%, rgba(251, 51, 27, 1) 100%);
  box-shadow: 0px 2px 3px 0px rgba(160, 18, 0, 0.56);
  color: #fff;
}

.invitation-record .record-card .card-box .btn.edit {
  background: rgba(255, 237, 197, 1);
  color: #ff9452;
}

.invitation-record .record-card .default {
  padding: 24rpx 0;
  text-align: center;
}

.invitation-record .record-card .default image {
  width: 234rpx;
}

.invitation-record .record-card .default .text {
  font-size: 24rpx;
  color: #ffca56;
  margin-top: 26rpx;
}

