/* packageB/invitation-share/invitation-share.wxss */

page {
  background-color: #ffe1a0;
}
.rule_txt{
  position: absolute;
  z-index: 9999;
  top: 20%;
  left: 50%;
  transform: translateX(-50%);
  -webkit-transform: translateX(-50%);
  font-size: 26rpx;
  width: 80%;
  
}
.rule_txt .itemtxt{
  display: flex;
  /* align-items: center; */
  justify-content: space-around; 
  margin-bottom: 10rpx;
}
.rule_txt .itemtxt .txtru{
  width: 90%;
}
.rule_txt .dot{
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  background-color: #f6a800;
  float: left;
  text-align: center;
  line-height: 40rpx;
  color: #fff;
}
.statement{
  margin-top: 40rpx;
}
.statement text{
  text-indent: 2em;
  color: #666;
}
.invitation-share {
  background-color: #ffe1a0;
  background-size: 100% 100%;
  padding-bottom: 40rpx;
}

.header-swiper {
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 64rpx;
}

.header-swiper .header-swiper-list {
  width: 750rpx;
  height: 64rpx;
  background: rgba(255, 242, 213, 0.48);
}

.header-swiper .header-swiper-list .swiper-item .swiper-block {
  height: 64rpx;
  justify-content: center;
  align-items: center;
}

.header-swiper .header-swiper-list .swiper-item .swiper-block .img {
  width: 52rpx;
  height: 52rpx;
}

.header-swiper .header-swiper-list .swiper-item .swiper-block .text {
  margin-left: 24rpx;
  font-size: 28rpx;
  color: #555;
  width: 500rpx;
}
.header-swiper .header-swiper-list .swiper-item .swiper-block .text .name{
  width: 100rpx;
  height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.header-swiper .header-swiper-list .swiper-item .swiper-block .text i {
  margin-left: 10rpx;
}

.header-swiper .header-swiper-list .swiper-item .swiper-block .price {
  font-size: 32rpx;
  color: #eb3c39;
  font-weight: 600;
  margin-left: 10rpx;
}

.red-log {
  position: absolute;
  top: 10%;
  width: 130rpx;
  height: 48rpx;
  text-align: center;
  line-height: 48rpx;
  left: 0;
  background: rgba(0, 0, 0, 0.37);
  border-radius: 0px 100px 100px 0px;
  font-size: 24rpx;
  color: #fff;
}

.red-activity {
  position: absolute;
  top: 10%;
  width: 130rpx;
  height: 48rpx;
  text-align: center;
  line-height: 48rpx;
  right: 0;
  background: rgba(0, 0, 0, 0.37);
  border-radius: 100px 0px 0px 100px;
  font-size: 24rpx;
  color: #fff;
}

.header-title {
  padding: 0 24rpx;
  padding-top: 100rpx;
  text-align: center;
}

.header-title .title {
  font-size: 52rpx;
  color: #303030;
  font-weight: 600;
  line-height: 74rpx;
  text-shadow: 0px 2px 2px rgba(255, 208, 107, 1);
}

