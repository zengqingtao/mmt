/* packageB/withdraw/withdraw.wxss */
.withdraw{
  background-color: #ffffff;
  height: 100vh
}
.header-main{
  width: 750rpx;
  height: 350rpx;
  background-size: 100% 100%;
  text-align: center;
}
.header-main .title{
  padding-top: 42rpx;
  font-size: 44rpx;
  font-weight:600;
  letter-spacing: 2px;
  color: #FFCBA8;
}
.header-main .price{
  font-size: 72rpx;
  font-weight: 600;
  color: #FFCBA8;
  margin-top: 20rpx;
  letter-spacing: 2px;
}
.header-main .price i{
  font-size: 44rpx;
  letter-spacing: 2px;
}
.swiper-list{
  position: relative;
  margin: 40rpx 24rpx;
  height: 400rpx;
}
.swiper-list .swiper-no-hove{
  width: 100%;
  height: 400rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.swiper-list .swiper-box{
  height: 400rpx;
}
.swiper-list .swiper-box .swiper-item{
  justify-content: center;
  align-items: center;
}
.swiper-list .swiper-box .swiper-item .item{
  padding: 30rpx 0;
  border-bottom: 1rpx #F0F0F0 solid;
  justify-content: center;
  align-items: center;
}
.swiper-list .swiper-box .swiper-item .logo{
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
  background-color: #eeeeee;
}
.swiper-list .swiper-box .swiper-item .text{
  font-size: 28rpx;
  color: #AAAAAA;
  margin-left: 24rpx;
  width: 500rpx;
  height: 44rpx;
  overflow: hidden;
}
.swiper-list .swiper-box .swiper-item .text .name{
  width: 100rpx;
  height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.swiper-list .swiper-box .swiper-item .text i{
  font-size: 26rpx;
  color: #303030;
  margin-left: 24rpx;
}
.swiper-list .swiper-box .swiper-item .price{
  font-size: 36rpx;
  font-weight: 600;
  width: 100rpx;
  color: #303030;
  text-align: right;
}
.swiper-list .swiper-box .swiper-item .price i{
  color: #EB3C39;
}
.footer-btn{
  position: relative;
  margin-top: 120rpx;
}
.footer-btn button{
  width: 702rpx;
  height: 96rpx;
  color: #FFFFFF;
  font-size: 48rpx;
  background:linear-gradient(180deg,rgba(255,106,88,1) 0%,rgba(251,51,27,1) 100%);
  box-shadow:0px 2px 3px 0px rgba(160,18,0,0.56);
  text-align: center;
  line-height: 96rpx;
  border-radius: 50rpx;
}
.footer-btn .prompt-img{
  position: absolute;
  right: 152rpx;
  top: 50%;
  transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
  width: 140rpx;
}

.curtain {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 99998;
}


.popup-model {
  position: fixed;
  top: 40%;
  left: 50%;
  transform: translate(-50%, -40%);
  -webkit-transform: translate(-50%, -40%);
  width: 665rpx;
  background-color: #fff;
  z-index: 99999;
  text-align: center;
  padding: 84rpx 0 64rpx 0;
  border-radius: 8rpx;
}

.popup-model .title{
  font-size: 52rpx;
  color: #303030
}
.popup-model .price-msg{
  display: flex;
  margin-top: 40rpx;
  align-items: center;
  justify-content: center;
}
.popup-model .price-msg .price{
  font-size: 68rpx;
  color: #EB3C39
}
.popup-model .price-msg .text{
  font-size: 36rpx;
  color: #303030;
  font-weight:600;
  margin-left: 12rpx;
}
.popup-model .text-log{
  font-size: 36rpx;
  color: #303030;
  margin-top: 26rpx;
  font-weight:600;
}
.popup-model .popup-footer-btn {
  padding-top: 78rpx;
  align-items: center;
  justify-content: center;
}

.popup-model .popup-footer-btn .btn {
  width: 284rpx;
  height: 84rpx;
  text-align: center;
  line-height: 84rpx;
  border-radius: 50px;
}

.popup-model .popup-footer-btn .btn.left {
  background-color: #FFFBEA;
  color: #905521;
  font-size: 30rpx;
}

.popup-model .popup-footer-btn .btn.right {
  background-color: #FFE150;
  color: #905521;
  font-size: 30rpx;
  margin-left: 24rpx;
}
