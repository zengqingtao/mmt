/* pages/user/shoucang.wxss */

.container {
  background-color: #FFF;
}


.attend {
  margin-bottom: 100rpx;
}

.top-subbar {
  width: 100%;
  background: #fff;
  position: relative;
  display: block;
  box-sizing: border-box;
  border-bottom: 2rpx #f2f2f2 solid;
  padding: 0 24rpx;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 30.8rpx;
}

.top-subbar .tr {
  color: #FF7F95;
}
.top-subbar .fl {
  color: #767676;
}

.list-item {
  position: relative;
  display: block;
  margin-left: 24rpx;
  height: 230rpx;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  border-bottom: #E9E9E9 1rpx solid;
  background-color: #ffffff;
}

.list-item icon {
  display: none;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
  left: -70rpx;
}

.marginLeft {
  margin-left: 90rpx;
}
.marginLeft icon {
  display: inline-block;
}

.item-image {
  position: absolute;
  top: 50%;
  left: 0;
  transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
  display: block;
  width: 180rpx;
  height: 180rpx;
  box-sizing: border-box;
  overflow: hidden;
}

.item-info {
  position: relative;
  display: block;
  height: 100%;
  margin-left: 208rpx;
}

.item-info .info-price {
  display: block;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding-top: 44rpx;
  color: #000000;
  font-size: 34rpx;
  line-height: 34rpx;
}

.item-info .info-name {
  display: block;
  position: relative;
  color: #000000;
  font-size: 24rpx;
  text-align: left;
  line-height: 28rpx;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding-right: 24rpx;
  padding-top: 14rpx;
}

.bottomTab {
  position: fixed;
  bottom: 0;
  height: 100rpx;
  background: #fff;
  width: 750rpx;
}

.cancelAtten {
  display: inline-block;
  width: 33%;
  height: 100%;
  text-align: center;
  position: absolute;
  right: 0rpx;
}

.selectIcon {
  display: inline-flex;
  width: 90rpx;
  height: 100rpx;
  justify-content: center;
  align-items: center;
  position: absolute;
}

.selectAll {
  position: absolute;
  line-height: 100rpx;
  font-size: 28rpx;
  margin-left: 90rpx;
  display: inline-block;
  width: 55%;
  height: 100rpx;
}

.loaded {
  display: block;
  width: 100%;
  height: 100rpx;
  box-sizing: border-box;
  color: #999;
  font-size: 26rpx;
  font-weight: 400;
  line-height: 100rpx;
  text-align: center;
  position: relative;
  top: -2rpx;
}
