.container.detail-loader {
  padding-bottom: 164rpx;
}
.order-info {
  padding: 32rpx 26rpx;
  position: relative;
  display: block;
  width: 100%;
  box-sizing: border-box;
  background-color: #fff;
}
.order-info.status-header{
  background-color: #ffe150;
  margin: 0;
  padding: 42rpx 100rpx 50rpx 100rpx;
}
.order-info.status-header .header-status{
  color: #303030;
  font-weight: 500;
  font-size: 30rpx;
}
.order-info.status-header .header-status-tip {
  color: #FFFFFF;
  font-size: 24rpx;
}
.group-time{
  color: #555;
  font-size: 24rpx;
}
.order-info.status-header .group-time text{
  color:#EB3C39;
  font-size: 24rpx;
}
.group-bag{
  width: 96rpx;
  height:96rpx;
}
.order-info.status-header .info-online{
  position: absolute;
  right: 100rpx;
  top: 50%;
  transform: translateY(-50%);
}
.order-info.status-header .info-online .iconfont{
  font-size:100rpx;

  color: #303030;

}
