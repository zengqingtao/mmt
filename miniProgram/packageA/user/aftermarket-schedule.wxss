/* pages/user/aftermarket-schedule.wxss */

.aftermarket-status {
  position: relative;
  display: block;
  width: 100%;
  box-sizing: border-box;
  padding: 54rpx 0;
}

.aftermarket-box {
  position: relative;
  display: block;
  padding: 0 26rpx 54rpx 120rpx;
}

.aftermarket-box .box-line {
  position: absolute;
  top: 0;
  left: 66rpx;
  width: 1px;
  height: 100%;
  background: #bbb;
}

.aftermarket-box .box-line:after {
  content: "";
  position: absolute;
  top: 0;
  left: 50%;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
  width: 26rpx;
  height: 26rpx;
  background: #bbb;
  border-radius: 50%;
}

.aftermarket-box.last .box-line {
  height: 0;
}

.aftermarket-box.last .box-line:after {
  background: #FF7F95;
}

.aftermarket-box.last.end .box-line:after {
  background: #FF7F95;
}

.aftermarket-box .box-top {
  line-height: 26rpx;
  margin-bottom: 10rpx;
}

.aftermarket-box .box-top i {
  font-size: 26rpx;
  line-height: 26rpx;
  color: #767676;
}

.aftermarket-box.last .box-top i {
  color: #FF7F95;
}

.aftermarket-box.last.end .box-top i {
  color: #FF7F95;
}

.aftermarket-box .box-main {
  padding: 30rpx 26rpx;
  background: #fff;
  position: relative;
}


.aftermarket-box .box-main.revist:after {
  content: '';
  position: absolute;
  left: -1px;
  top: 50%;
  transform: translate(-50%, -50%) rotate(45deg);
  -webkit-transform: translate(-50%, -50%) rotate(45deg);
  width: 22rpx;
  height: 22rpx;
  background: #fff;
}

.aftermarket-box .box-main.revive:after {
  content: '';
  position: absolute;
  right: -1px;
  top: 50%;
  transform: translate(50%, -50%) rotate(45deg);
  -webkit-transform: translate(50%, -50%) rotate(45deg);
  width: 22rpx;
  height: 22rpx;
  background: #fff;
}

.aftermarket-box .box-main .main_title {
  font-size: 30rpx;
  line-height: 30rpx;
  color: #030303;
  padding-bottom: 16rpx;
}

.aftermarket-box .box-main .main_title.no-padding {
  padding-bottom: 0;
}

.aftermarket-box .box-main .main_list_menu01 .main_list_menu01_p {
