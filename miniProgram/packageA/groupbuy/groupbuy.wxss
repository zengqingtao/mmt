@import "/wxParse/suui/suui.wxss";
page {
  background-color: #fff;
}

/*商品信息*/

.goods-msg {
  width: 100%;
  padding: 64rpx 24rpx;
  box-sizing: border-box;
  background-color: #fff;
  /* margin-bottom:16rpx; */
}

.goods-msg .goods-box {
  width: 100%;
  padding: 24rpx;
  display: flex;
  box-sizing: border-box;
  box-shadow: 0rpx 0rpx 10px 0rpx rgba(42, 42, 42, 0.08);
}

.goods-msg .goods-img {
  width: 160rpx;
  height: 160rpx;
}
.goods-none{
  width: 160rpx;
  height: 160rpx;
  position: absolute;
  top:88rpx;
  left: 48rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(0,0,0,0.3)
}
.font_24 {
font-size: 24rpx;
}

.goods-none image{
  width: 126rpx;
  height:52rpx;
}
.goods-msg .goods-news {
  margin-left: 16rpx;
  flex: 1;
  position: relative;
}

.goods-msg .goods-news .goods-name {
  overflow: hidden;
  font-weight: 600;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  text-overflow: -o-ellipsis-lastline;
}

.goods-msg .goods-news .goods-standrad {
  margin-top: 4rpx;
  font-size: 24rpx;
  color: #aaa;
}

.goods-msg .goods-news .group-price {
  position: absolute;
  left: 0;
  bottom: 0;
  display: flex;
  align-items: center
}
.goods-msg .goods-news .group-price .price-sign{
  font-size: 24rpx;
  color: #eb3c39;
  font-weight: 500;
}
.goods-msg .goods-news .group-price .price {
  font-size: 32rpx;
  color: #eb3c39;
  font-weight: 500;
}

.goods-msg .goods-news .group-price .cost-price {
  font-size: 24rpx;
  color: #b5b5b5;
  margin-left: 6rpx;
  text-decoration: line-through;
  font-weight: 500;
}

/*拼团信息*/

.group-msg {
  background-color: #fff;
  padding: 0 24rpx;
}

.group-msg .group-time {
  text-align: center;
}

.group-msg .group-time .short {
  color: #303030;
  font-size: 40rpx;
  font-weight: 500;
}

.group-msg .group-time .short-time {
  color: #303030;
  font-size: 24rpx;
  font-weight: 500;
  margin-top: 18rpx;
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  justify-content: center;
}

.group-msg .group-time .short-time .time-box {
  width: 44rpx;
  height: 36rpx;
  background-color: #f2f2f2;
  border-radius: 8rpx;
  text-align: center;
  line-height: 36rpx;
  margin: 0 10rpx;
}

.group-msg .group-time .short-time .dot {
  font-size: 24rpx;
  color: #545454;
  font-weight: 600;
}

.group-msg .group-time .short-time .time-box text {
  color: #eb3c39;
  font-weight: 600;
}

.group-msg .group-time .group-success {
  font-size: 24rpx;
  margin-top: 16rpx;
  color:#303030;
}

.group-msg  .group-team {
  display: flex;
  padding: 48rpx 0 86rpx 0;
  width: 100%;
  justify-content: center;
  flex-wrap: wrap;
}

.group-msg  .group-team .member {
  /*flex: 20%;*/
  padding: 0 12rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

.group-msg  .group-team .member .add-member {
  text-align: center;
  line-height: 96rpx;
  border: 2rpx #ffe150 dashed;
  background-color: #fffbe6;
  height: 96rpx;
  width: 96rpx;
  box-sizing: border-box;
  border-radius: 50%;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}

.group-msg  .group-team .member .add-member .row {
  background-color: #ffe150;
  width: 36rpx;
  height: 2rpx;
}

.group-msg  .group-team .member .add-member .col {
  background-color: #ffe150;
  width: 2rpx;
  height: 36rpx;
  position: absolute;
  margin: auto auto;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  /*transform: translateY(-50%);*/
}

.group-msg  .group-team .member image {
  height: 96rpx;
  width: 96rpx;
  border-radius: 50%;
  background-size: 100% 100%;
}

.group-msg  .group-team .member .captain {
  position: absolute;
  bottom: -10rpx;
  left: 50%;
  width: 72rpx;
  height: 28rpx;
  border-radius: 14rpx;
  transform: translateX(-50%);
  color: #303030;
  background-color: #ffe150;
  text-align: center;
  font-size: 20rpx;
}

.group-msg .get-group {
  width: 100%;
  height: 78rpx;
  background-color: #ffe150;
  border-radius: 50rpx;
  color: #555;
  font-size: 28rpx;
  text-align: center;
  line-height: 78rpx;
  font-weight: 600;
}

.group-msg .get-group.go-home {
  margin-top: 32rpx;
  background-color: #F2F2F2;
}

/*拼单跳转*/

.my-group {
  width: 100%;
  box-sizing: border-box;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  /* margin-top: 48rpx; */
  color: #aaa;
  font-size: 28rpx;
  font-weight: 500;
  padding: 48rpx 0 20rpx 0;
}

/* 活动规则弹窗 */

.rules-pop-hold .rules-pop-md {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 99;
}

.rules-pop-hold .rules-pop {
  width: 500rpx;
  height: 750rpx;
  border-radius: 8px;
  z-index: 100;
  position: fixed;
  background: #ffe150;
  top: 126rpx;
  left: 100rpx;
  padding: 0rpx 24rpx;
}

.rules-pop-hold .rules-pop .rules-content {
  position: relative;
}

.rules-pop-hold .rules-title {
  font-size: 40rpx;
  color: #555;
  text-align: center;
  margin-top: 50rpx;
}

.rules-pop-hold .rules-pop .rules-text-box {
  background-color: #fff;
  border-radius: 8rpx;
  /* margin: 0rpx 24rpx; */
  padding: 40rpx 0;
  height: 500rpx;
  margin-top: 20rpx;
}

.rules-text-box text {
  display: block;
  font-size: 28rpx;
  padding: 0 40rpx;
  margin-bottom: 20rpx;
}

.rules-line {
  width: 64rpx;
  height: 8rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 13rpx;
}

.rules-content .icon-closs {
  font-size: 58rpx;
  color: #fff;
  position: absolute;
  bottom: -120rpx;
  line-height: 1.2;
  left: 50%;
  transform: translateX(-50%);
  background-color: transparent;
}

.show {
  display: block;
}

.hide {
  display: none;
}

