.join-team{
  width: 100%;
  padding: 24rpx;
  background-color: #fff;
  box-sizing: border-box;
  margin-top: 16rpx;
  margin-bottom: 60rpx;
}
.join-team .join-header{
  padding-bottom: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size:28rpx;
  color:#333;
  font-weight: 600;
  box-sizing: border-box;
  border-bottom: 2rpx solid #f0f0f0;
}
.join-team .join-list{
  padding: 24rpx 0;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  border-bottom: 2rpx solid #f0f0f0;
  position: relative;

}
.join-team .join-list .join-list-img{
  width:72rpx;
  height:72rpx;
  background-color: gainsboro;
}
.join-team .join-list .join-list-name{
  font-size: 24rpx;
  color:#555;
  margin-left: 24rpx;
}
.join-list-right{
  width: 400rpx;
  display: flex;
  justify-content: space-between;
  position: absolute;
  right: 0;
  top:50%;
  transform: translateY(-50%)
}
.join-team .join-list .join-list-less{
  text-align: center;
  font-size: 20rpx;
