  display: inline-block;
}
.join-team .join-list .join-list-less .i{
  color:#eb3c39;
}
.join-list-btn{
  width:160rpx;
  height:60rpx;
  color:#555;
  font-weight: 600;
  font-size: 24rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 30rpx;
  background-color:#ffe150;
}
.team-list-box{
  width:100%;
  max-height:244rpx;
